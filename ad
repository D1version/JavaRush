[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        Date date = new Date(reader.readLine());[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        SimpleDateFormat formate = new SimpleDateFormat("MMM dd, y", Locale.ENGLISH);[m[41m[m
[32m+[m[32m        System.out.println(formate.format(date).toUpperCase());[m[41m[m
