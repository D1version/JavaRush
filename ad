[32m+[m[32mimport java.util.Set;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ú–Ω–æ–∂–µ—Å—Ç–≤–æ –≤—Å–µ—Ö –∂–∏–≤–æ—Ç–Ω—ã—Ö[m[41m[m
[32m+[m[32m1. –í–Ω—É—Ç—Ä–∏ –∫–ª–∞—Å—Å–∞ Solution —Å–æ–∑–¥–∞—Ç—å public static –∫–ª–∞—Å—Å—ã Cat, Dog.[m[41m[m
[32m+[m[32m2. –†–µ–∞–ª–∏–∑–æ–≤–∞—Ç—å –º–µ—Ç–æ–¥ createCats, –∫–æ—Ç–æ—Ä–æ—ã–π –¥–æ–ª–∂–µ–Ω –≤–æ–∑–≤—Ä–∞—â–∞—Ç—å –º–Ω–æ–∂–µ—Å—Ç–≤–æ —Å 4 –∫–æ—Ç–∞–º–∏.[m[41m[m
[32m+[m[32m3. –†–µ–∞–ª–∏–∑–æ–≤–∞—Ç—å –º–µ—Ç–æ–¥ createDogs, –∫–æ—Ç–æ—Ä–æ—ã–π –¥–æ–ª–∂–µ–Ω –≤–æ–∑–≤—Ä–∞—â–∞—Ç—å –º–Ω–æ–∂–µ—Å—Ç–≤–æ —Å 3 —Å–æ–±–∞–∫–∞–º–∏.[m[41m[m
[32m+[m[32m4. –†–µ–∞–ª–∏–∑–æ–≤–∞—Ç—å –º–µ—Ç–æ–¥ join, –∫–æ—Ç–æ—Ä–æ—ã–π –¥–æ–ª–∂–µ–Ω –≤–æ–∑–≤—Ä–∞—â–∞—Ç—å –æ–±—ä–µ–¥–∏–Ω–µ–Ω–Ω–æ–µ –º–Ω–æ–∂–µ—Å—Ç–≤–æ –≤—Å–µ—Ö –∂–∏–≤–æ—Ç–Ω—ã—Ö - –≤—Å–µ—Ö –∫–æ—Ç–æ–≤ –∏ —Å–æ–±–∞–∫.[m[41m[m
[32m+[m[32m5. –†–µ–∞–ª–∏–∑–æ–≤–∞—Ç—å –º–µ—Ç–æ–¥ removeCats, –∫–æ—Ç–æ—Ä–æ—ã–π –¥–æ–ª–∂–µ–Ω —É–¥–∞–ª—è—Ç—å –∏–∑ –º–Ω–æ–∂–µ—Å—Ç–≤–∞ pets –≤—Å–µ—Ö –∫–æ—Ç–æ–≤, –∫–æ—Ç–æ—Ä—ã–µ –µ—Å—Ç—å –≤ –º–Ω–æ–∂–µ—Å—Ç–≤–µ cats.[m[41m[m
[32m+[m[32m6. –†–µ–∞–ª–∏–∑–æ–≤–∞—Ç—å –º–µ—Ç–æ–¥ printPets, –∫–æ—Ç–æ—Ä–æ—ã–π –¥–æ–ª–∂–µ–Ω –≤—ã–≤–æ–¥–∏—Ç—å –Ω–∞ —ç–∫—Ä–∞–Ω –≤—Å–µ—Ö –∂–∏–≤–æ—Ç–Ω—ã—Ö, –∫–æ—Ç–æ—Ä—ã–µ –≤ –Ω–µ–º –µ—Å—Ç—å. –ö–∞–∂–¥–æ–µ –∂–∏–≤–æ—Ç–Ω–æ–µ —Å –Ω–æ–≤–æ–π —Å—Ç—Ä–æ–∫–∏[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Set<Cat> cats = createCats();[m[41m[m
[32m+[m[32m        Set<Dog> dogs = createDogs();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        Set<Object> pets = join(cats, dogs);[m[41m[m
[32m+[m[32m        printPets(pets);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        removeCats(pets, cats);[m[41m[m
[32m+[m[32m        printPets(pets);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static Set<Cat> createCats()[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashSet<Cat> result = new HashSet<Cat>();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        result.add(Cat.cat1);[m[41m[m
[32m+[m[32m        result.add(Cat.cat2);[m[41m[m
[32m+[m[32m        result.add(Cat.cat3);[m[41m[m
[32m+[m[32m        result.add(Cat.cat4);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return result;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static Set<Dog> createDogs()[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashSet<Dog> result = new HashSet<Dog>();[m[41m[m
[32m+[m[32m        result.add(Dog.dog1);[m[41m[m
[32m+[m[32m        result.add(Dog.dog2);[m[41m[m
[32m+[m[32m        result.add(Dog.dog3);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return result;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static Set<Object> join(Set<Cat> cats, Set<Dog> dogs)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashSet<Object> join = new HashSet<Object>();[m[41m[m
[32m+[m[32m        join.addAll(cats);[m[41m[m
[32m+[m[32m        join.addAll(dogs);[m[41m[m
[32m+[m[32m        return join;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void removeCats(Set<Object> pets, Set<Cat> cats)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (Cat x : cats)[m[41m[m
[32m+[m[32m            pets.remove(x);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void printPets(Set<Object> pets)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (Object x : pets)[m[41m[m
[32m+[m[32m            System.out.println(x);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static class Cat[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        static Cat cat1 = new Cat();[m[41m[m
[32m+[m[32m        static Cat cat2 = new Cat();[m[41m[m
[32m+[m[32m        static Cat cat3 = new Cat();[m[41m[m
[32m+[m[32m        static Cat cat4 = new Cat();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static class Dog[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        static Dog dog1 = new Dog();[m[41m[m
[32m+[m[32m        static Dog dog2 = new Dog();[m[41m[m
[32m+[m[32m        static Dog dog3 = new Dog();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
