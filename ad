[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ArrayList<String> list = new ArrayList<String>();[m[41m[m
[32m+[m[32m        while (true)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String s = reader.readLine();[m[41m[m
[32m+[m[32m            if (s.isEmpty()) break;[m[41m[m
[32m+[m[32m            list.add(s);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ArrayList<String> newList = new ArrayList<String>();[m[41m[m
[32m+[m[32m        newList = list;[m[41m[m
[32m+[m[32m        for (int i = 0; i < newList.size(); i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int s = list.get(i).length();[m[41m[m
[32m+[m[32m            if (s %2 == 0){[m[41m[m
[32m+[m[32m                newList.add(i, list.get(i) + " " + list.get(i));[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else {[m[41m[m
[32m+[m[32m                newList.add(i, list.get(i) + " " + list.get(i) + " " + list.get(i));[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (int i = 0; i < newList.size(); i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(newList.get(i));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
