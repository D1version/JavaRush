[32m+[m[32m5. –í—ã–≤–µ—Å—Ç–∏ –≤—Ç–æ—Ä–æ–π –º–∞–ª–µ–Ω—å–∫–∏–π –º–∞—Å—Å–∏–≤ –Ω–∞ —ç–∫—Ä–∞–Ω, –∫–∞–∂–¥–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ –≤—ã–≤–æ–¥–∏—Ç—å —Å –Ω–æ–≤–æ–π —Å—Ç—Ä–æ–∫–∏.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        int[] massiv = new int[20];[m[41m[m
[32m+[m[32m        for(int i = 0; i < 20; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String s = reader.readLine();[m[41m[m
[32m+[m[32m            massiv[i] = Integer.parseInt(s);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int[] massiv1 = new int[10];[m[41m[m
[32m+[m[32m        int[] massiv2 = new int[10];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for(int i = 0; i < 10; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            massiv1[i] = massiv[i];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        massiv2[0] = massiv[10];[m[41m[m
[32m+[m[32m        massiv2[1] = massiv[11];[m[41m[m
[32m+[m[32m        massiv2[2] = massiv[12];[m[41m[m
[32m+[m[32m        massiv2[3] = massiv[13];[m[41m[m
[32m+[m[32m        massiv2[4] = massiv[14];[m[41m[m
[32m+[m[32m        massiv2[5] = massiv[15];[m[41m[m
[32m+[m[32m        massiv2[6] = massiv[16];[m[41m[m
[32m+[m[32m        massiv2[7] = massiv[17];[m[41m[m
[32m+[m[32m        massiv2[8] = massiv[18];[m[41m[m
[32m+[m[32m        massiv2[9] = massiv[19];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for(int i = 0; i < 10; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(massiv2[i]);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
