[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ArrayList<Integer> list = new ArrayList<Integer>();[m[41m[m
[32m+[m[32m        for(int i = 0; i < 10;i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String s = reader.readLine();[m[41m[m
[32m+[m[32m            int a = Integer.parseInt(s);[m[41m[m
[32m+[m[32m            list.add(a);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for(int i = 0; i < 10; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int j = list.size() - i - 1;[m[41m[m
[32m+[m[32m            System.out.println( list.get(j) );[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
