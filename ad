[32m+[m[41m[m
[32m+[m[32mimport java.util.HashMap;[m[41m[m
[32m+[m[32mimport java.util.Map;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –í—ã–≤–µ—Å—Ç–∏ –Ω–∞ —ç–∫—Ä–∞–Ω —Å–ø–∏—Å–æ–∫ –∑–Ω–∞—á–µ–Ω–∏–π[m[41m[m
[32m+[m[32m–ï—Å—Ç—å –∫–æ–ª–ª–µ–∫—Ü–∏—è HashMap<String, String>, —Ç—É–¥–∞ –∑–∞–Ω–µ—Å–ª–∏ 10 —Ä–∞–∑–ª–∏—á–Ω—ã—Ö —Å—Ç—Ä–æ–∫.[m[41m[m
[32m+[m[32m–í—ã–≤–µ—Å—Ç–∏ –Ω–∞ —ç–∫—Ä–∞–Ω —Å–ø–∏—Å–æ–∫ –∑–Ω–∞—á–µ–Ω–∏–π, –∫–∞–∂–¥—ã–π —ç–ª–µ–º–µ–Ω—Ç —Å –Ω–æ–≤–æ–π —Å—Ç—Ä–æ–∫–∏.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashMap<String, String> map = new HashMap<String, String>();[m[41m[m
[32m+[m[32m        map.put("Sim", "Sim");[m[41m[m
[32m+[m[32m        map.put("Tom", "Tom");[m[41m[m
[32m+[m[32m        map.put("Arbus", "Arbus");[m[41m[m
[32m+[m[32m        map.put("Baby", "Baby");[m[41m[m
[32m+[m[32m        map.put("Cat", "Cat");[m[41m[m
[32m+[m[32m        map.put("Dog", "Dog");[m[41m[m
[32m+[m[32m        map.put("Eat", "Eat");[m[41m[m
[32m+[m[32m        map.put("Food", "Food");[m[41m[m
[32m+[m[32m        map.put("Gevey", "Gevey");[m[41m[m
[32m+[m[32m        map.put("Hugs", "Hugs");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        printValues(map);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void printValues(Map<String, String> map)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (Map.Entry<String, String> pair : map.entrySet())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String key = pair.getValue();[m[41m[m
[32m+[m[32m            System.out.println(key);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
