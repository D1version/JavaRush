[32m+[m[32m–ü–æ—è—Å–Ω–µ–Ω–∏–µ:[m[41m[m
[32m+[m[32m–°–∞–º–æ–µ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ —á–∏—Å–ª–æ ‚Äì 1-–µ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ.[m[41m[m
[32m+[m[32m–°–ª–µ–¥—É—é—â–µ–µ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ –ø–æ—Å–ª–µ –Ω–µ–≥–æ ‚Äì 2-–µ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ[m[41m[m
[32m+[m[32m–ü—Ä–∏–º–µ—Ä:[m[41m[m
[32m+[m[32m1 6 5  7  1  15   63   88[m[41m[m
[32m+[m[32m–ü–µ—Ä–≤–æ–µ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ ‚Äì 1[m[41m[m
[32m+[m[32m–í—Ç–æ—Ä–æ–µ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ ‚Äì 1[m[41m[m
[32m+[m[32m–¢—Ä–µ—Ç—å–µ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ ‚Äì 5[m[41m[m
[32m+[m[32m–ß–µ—Ç–≤–µ—Ä—Ç–æ–µ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ ‚Äì 6[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        int[] array = new int[30];[m[41m[m
[32m+[m[32m        for (int i = 0; i < 30; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            array[i] = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        sort(array);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        System.out.println(array[9]);[m[41m[m
[32m+[m[32m        System.out.println(array[10]);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void sort(int[] array)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (int i = 1; i<array.length; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int key = array[i];[m[41m[m
[32m+[m[32m            int j = i-1;[m[41m[m
[32m+[m[32m            while (j >=0 && array[j] > key)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                array[j + 1] = array[j];[m[41m[m
[32m+