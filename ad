[32m+[m[32m–ó–∞–¥–∞—á–∞: –ü—Ä–æ–≥—Ä–∞–º–º–∞ –≤–≤–æ–¥–∏—Ç –¥–≤–∞ –∏–º–µ–Ω–∏ —Ñ–∞–π–ª–∞. –ò –∫–æ–ø–∏—Ä—É–µ—Ç –ø–µ—Ä–≤—ã–π —Ñ–∞–π–ª –Ω–∞ –º–µ—Å—Ç–æ, –∑–∞–¥–∞–Ω–Ω–æ–µ –≤—Ç–æ—Ä—ã–º –∏–º–µ–Ω–µ–º.[m[41m[m
[32m+[m[32m–ù–æ–≤–∞—è –∑–∞–¥–∞—á–∞: –ü—Ä–æ–≥—Ä–∞–º–º–∞ –≤–≤–æ–¥–∏—Ç –¥–≤–∞ –∏–º–µ–Ω–∏ —Ñ–∞–π–ª–∞. –ò –∫–æ–ø–∏—Ä—É–µ—Ç –ø–µ—Ä–≤—ã–π —Ñ–∞–π–ª –Ω–∞ –º–µ—Å—Ç–æ, –∑–∞–¥–∞–Ω–Ω–æ–µ –≤—Ç–æ—Ä—ã–º –∏–º–µ–Ω–µ–º.[m[41m[m
[32m+[m[32m–ï—Å–ª–∏ —Ñ–∞–π–ª–∞ (–∫–æ—Ç–æ—Ä—ã–π –Ω—É–∂–Ω–æ –∫–æ–ø–∏—Ä–æ–≤–∞—Ç—å) —Å —É–∫–∞–∑–∞–Ω–Ω—ã–º –∏–º–µ–Ω–µ–º –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç, —Ç–æ[m[41m[m
[32m+[m[32m–ø—Ä–æ–≥—Ä–∞–º–º–∞ –¥–æ–ª–∂–Ω–∞ –≤—ã–≤–µ—Å—Ç–∏ –Ω–∞–¥–ø–∏—Å—å ¬´–§–∞–π–ª –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç.¬ª –∏ –µ—â–µ —Ä–∞–∑ –ø—Ä–æ—á–∏—Ç–∞—Ç—å –∏–º—è —Ñ–∞–π–ª–∞ —Å –∫–æ–Ω—Å–æ–ª–∏, –∞ —É–∂–µ –ø–æ—Ç–æ–º —Å—á–∏—Ç—ã–≤–∞—Ç—å —Ñ–∞–π–ª –¥–ª—è –∑–∞–ø–∏—Å–∏.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        FileInputStream fileInputStream = null;[m[41m[m
[32m+[m[32m        FileOutputStream fileOutputStream = null;[m[41m[m
[32m+[m[32m        while (true)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                String sourceFileName = reader.readLine();[m[41m[m
[32m+[m[32m                fileInputStream = new FileInputStream(sourceFileName);[m[41m[m
[32m+[m[32m                break;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (FileNotFoundException e)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                System.out.println("–§–∞–π–ª –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç.");[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        String destinationFileName = reader.readLine();[m[41m[m
[32m+[m[32m        fileOutputStream = new FileOutputStream(destinationFileName);[m[41m[m
[32m+[m[32m        while (fileInputStream.available() > 0)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int data = fileInputStream.read();[m[41m[m
[32m+[m[32m            fileOutputStream.write(data);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        fileInputStream.close();[m[41m[m
[32m+[m[32m        fileOutputStream.close();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
