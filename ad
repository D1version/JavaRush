[32m+[m[32m/* –£–¥–∞–ª–∏—Ç—å –≤—Å–µ —á–∏—Å–ª–∞ –±–æ–ª—å—à–µ 10[m[41m[m
[32m+[m[32m–°–æ–∑–¥–∞—Ç—å –º–Ω–æ–∂–µ—Å—Ç–≤–æ —á–∏—Å–µ–ª(Set<Integer>), –∑–∞–Ω–µ—Å—Ç–∏ —Ç—É–¥–∞ 20 —Ä–∞–∑–ª–∏—á–Ω—ã—Ö —á–∏—Å–µ–ª.[m[41m[m
[32m+[m[32m–£–¥–∞–ª–∏—Ç—å –∏–∑ –º–Ω–æ–∂–µ—Å—Ç–≤–∞ –≤—Å–µ —á–∏—Å–ª–∞ –±–æ–ª—å—à–µ 10.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static HashSet<Integer> createSet()[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashSet<Integer> set = new HashSet<Integer>();[m[41m[m
[32m+[m[32m        for (int i = 0; i < 20; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            set.add(i+1);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    return set;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static HashSet<Integer> removeAllNumbersMoreThan10(HashSet<Integer> set)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Iterator<Integer> itr = set.iterator();[m[41m[m
[32m+[m[32m        while(itr.hasNext()) {[m[41m[m
[32m+[m[32m            int i = itr.next();[m[41m[m
[32m+[m[32m            if (i > 10) {[m[41m[m
[32m+[m[32m                itr.remove();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return set;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
