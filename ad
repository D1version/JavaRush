[32m+[m[32m–í—ã–≤–µ—Å—Ç–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç –Ω–∞ —ç–∫—Ä–∞–Ω, –∫–∞–∂–¥—ã–π —ç–ª–µ–º–µ–Ω—Ç —Å –Ω–æ–≤–æ–π —Å—Ç—Ä–æ–∫–∏.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        String[] cats = new String[] {"–≤–∞—Å—å–∫–∞", "–º—É—Ä–∫–∞", "–¥—ã–º–∫–∞", "—Ä—ã–∂–∏–∫", "—Å–µ—Ä—ã–π", "—Å–Ω–µ–∂–æ–∫", "–±–æ—Å—Å", "–±–æ—Ä–∏—Å", "–≤–∏–∑—è", "–≥–∞—Ä—Ñ–∏"};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        HashMap<String, Cat> map = addCatsToMap(cats);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (Map.Entry<String, Cat> pair : map.entrySet())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(pair.getKey() + " - " + pair.getValue());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static HashMap<String, Cat> addCatsToMap(String[] cats)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashMap<String, Cat> map = new HashMap<String, Cat>();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for(String str : cats)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            map.put(str, new Cat(str));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return map;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static class Cat[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        String name;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public Cat(String name)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.name = name;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        @Override[m[41m[m
[32m+[m[32m        public String toString()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return name != null ? name.toUpperCase() : null;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
