[32m+[m[41m[m
[32m+[m[32m–ü—Ä–∏–º–µ—Ä –≤—ã–≤–æ–¥–∞:[m[41m[m
[32m+[m[32m1 –ú–∞–º–∞[m[41m[m
[32m+[m[32m2 –†–∞–º–∞[m[41m[m
[32m+[m[32m1 –ú—ã–ª–∞[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        HashMap<String, Integer> map = new HashMap<String, Integer>();[m[41m[m
[32m+[m[32m        while(true)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m        String id = reader.readLine();[m[41m[m
[32m+[m[32m            if(id.isEmpty())[m[41m[m
[32m+[m[32m                break;[m[41m[m
[32m+[m[32m        int idd = Integer.parseInt(id);[m[41m[m
[32m+[m[32m        String name = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            map.put(name, idd);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        Iterator<Map.Entry<String, Integer>> iterator = map.entrySet().iterator();[m[41m[m
[32m+[m[32m        while(iterator.hasNext())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            Map.Entry<String,Integer> pair = iterator.next();[m[41m[m
[32m+[m[32m            String key = pair.getKey();[m[41m[m
[32m+[m[32m            Integer value = pair.getValue();[m[41m[m
[32m+[m[32m            System.out.println(value + " " + key);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
