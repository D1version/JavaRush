[32m+[m[32m    public static void main(String[] args)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        System.out.println(getTimeMsOfInsert(new ArrayList()));[m[41m[m
[32m+[m[32m        System.out.println(getTimeMsOfInsert(new LinkedList()));[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static long  getTimeMsOfInsert(List list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Date start = new Date();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        insert10000(list);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        Date finish = new Date();[m[41m[m
[32m+[m[32m        long msDelay = finish.getTime() - start.getTime();[m[41m[m
[32m+[m[32m        System.out.print(msDelay);[m[41m[m
[32m+[m[32m        return msDelay;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void insert10000(List list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (int i=0;i<10000;i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.add(0, new Object());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
