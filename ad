[32m+[m[32mimport java.io.*;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –°—Ä–µ–¥–Ω–µ–µ —Ç–∞–∫–æ–µ —Å—Ä–µ–¥–Ω–µ–µ[m[41m[m
[32m+[m[32m–í–≤–µ—Å—Ç–∏ —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã —Ç—Ä–∏ —á–∏—Å–ª–∞, –≤—ã–≤–µ—Å—Ç–∏ –Ω–∞ —ç–∫—Ä–∞–Ω —Å—Ä–µ–¥–Ω–µ–µ –∏–∑ –Ω–∏—Ö. –¢.–µ. –Ω–µ —Å–∞–º–æ–µ –±–æ–ª—å—à–æ–µ –∏ –Ω–µ —Å–∞–º–æ–µ –º–∞–ª–µ–Ω—å–∫–æ–µ.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args)   throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        String aa = reader.readLine();[m[41m[m
[32m+[m[32m        String bb = reader.readLine();[m[41m[m
[32m+[m[32m        String cc = reader.readLine();[m[41m[m
[32m+[m[32m        int a = Integer.parseInt(aa);[m[41m[m
[32m+[m[32m        int b = Integer.parseInt(bb);[m[41m[m
[32m+[m[32m        int c = Integer.parseInt(cc);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if(a < b && b < c)[m[41m[m
[32m+[m[32m            System.out.print(b);[m[41m[m
[32m+[m[32m        else if(b < a && a < c)[m[41m[m
[32m+[m[32m            System.out.print(a);[m[41m[m
[32m+[m[32m        else if(a < c && c < b)[m[41m[m
[32m+[m[32m            System.out.print(c);[m[41m[m
[32m+[m[32m        else[m[41m[m
[32m+[m[32m            System.out.print(c);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
