[32m+[m[32m1. –í–≤–µ–¥–∏ —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã 20 —á–∏—Å–µ–ª, —Å–æ—Ö—Ä–∞–Ω–∏ –∏—Ö –≤ —Å–ø–∏—Å–æ–∫ –∏ —Ä–∞—Å—Å–æ—Ä—Ç–∏—Ä—É–π –ø–æ —Ç—Ä—ë–º –¥—Ä—É–≥–∏–º —Å–ø–∏—Å–∫–∞–º:[m[41m[m
[32m+[m[32m–ß–∏—Å–ª–æ –¥–µ–ª–∏—Ç—Å—è –Ω–∞ 3 (x%3==0), –¥–µ–ª–∏—Ç—Å—è –Ω–∞ 2 (x%2==0) –∏ –≤—Å–µ –æ—Å—Ç–∞–ª—å–Ω—ã–µ.[m[41m[m
[32m+[m[32m–ß–∏—Å–ª–∞, –∫–æ—Ç–æ—Ä—ã–µ –¥–µ–ª—è—Ç—Å—è –Ω–∞ 3 –∏ –Ω–∞ 2 –æ–¥–Ω–æ–≤—Ä–µ–º–µ–Ω–Ω–æ, –Ω–∞–ø—Ä–∏–º–µ—Ä 6, –ø–æ–ø–∞–¥–∞—é—Ç –≤ –æ–±–∞ —Å–ø–∏—Å–∫–∞.[m[41m[m
[32m+[m[32m2. –ú–µ—Ç–æ–¥ printList –¥–æ–ª–∂–µ–Ω –≤—ã–≤–æ–¥–∏—Ç—å –Ω–∞ —ç–∫—Ä–∞–Ω –≤—Å–µ —ç–ª–µ–º–µ–Ω—Ç—ã —Å–ø–∏—Å–∫–∞  —Å –Ω–æ–≤–æ–π —Å—Ç—Ä–æ–∫–∏.[m[41m[m
[32m+[m[32m3. –ò—Å–ø–æ–ª—å–∑—É—è –º–µ—Ç–æ–¥ printList –≤—ã–≤–µ–¥–∏ —ç—Ç–∏ —Ç—Ä–∏ —Å–ø–∏—Å–∫–∞ –Ω–∞ —ç–∫—Ä–∞–Ω. –°–Ω–∞—á–∞–ª–∞ —Ç–æ—Ç, –∫–æ—Ç–æ—Ä—ã–π –¥–ª—è x%3, –ø–æ—Ç–æ–º —Ç–æ—Ç, –∫–æ—Ç–æ—Ä—ã–π –¥–ª—è x%2, –ø–æ—Ç–æ–º –ø–æ—Å–ª–µ–¥–Ω–∏–π.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        ArrayList<Integer> list = new ArrayList<Integer>();[m[41m[m
[32m+[m[32m        for( int i = 0; i<20; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.add(Integer.parseInt(reader.readLine()));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        ArrayList<Integer> even =  new ArrayList<Integer>();[m[41m[m
[32m+[m[32m        ArrayList<Integer> odd =  new ArrayList<Integer>();[m[41m[m
[32m+[m[32m        ArrayList<Integer> but =  new ArrayList<Integer>();[m[41m[m
[32m+[m[32m        for(int x : list)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (x % 3 == 0) even.add(x);[m[41m[m
[32m+[m[32m            if (x % 2 == 0) odd.add(x);[m[41m[m
[32m+[m[32m            if(x % 2 != 0 && x % 3 != 0) but.add(x);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        printList(even);[m[41m[m
[32m+[m[32m        printList(odd);[m[41m[m
[32m+[m[32m        printList(but);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    public static void printList(List<Integer> list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (int i : list) System.out.println(i);[m[41m[m
[32m+[m[32m        //add your code here[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
