[32m+[m[32mimport java.io.BufferedReader;[m[41m[m
[32m+[m[32mimport java.io.IOException;[m[41m[m
[32m+[m[32mimport java.io.InputStreamReader;[m[41m[m
[32m+[m[32mimport java.util.ArrayList;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ü–µ—Ä–µ—Å—Ç–∞–≤–∏—Ç—å M –ø–µ—Ä–≤—ã—Ö —Å—Ç—Ä–æ–∫ –≤ –∫–æ–Ω–µ—Ü —Å–ø–∏—Å–∫–∞[m[41m[m
[32m+[m[32m–í–≤–µ—Å—Ç–∏ —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã 2 —á–∏—Å–ª–∞ N  –∏ M.[m[41m[m
[32m+[m[32m–í–≤–µ—Å—Ç–∏ N —Å—Ç—Ä–æ–∫ –∏ –∑–∞–ø–æ–ª–Ω–∏—Ç—å –∏–º–∏ —Å–ø–∏—Å–æ–∫.[m[41m[m
[32m+[m[32m–ü–µ—Ä–µ—Å—Ç–∞–≤–∏—Ç—å M –ø–µ—Ä–≤—ã—Ö —Å—Ç—Ä–æ–∫ –≤ –∫–æ–Ω–µ—Ü —Å–ø–∏—Å–∫–∞.[m[41m[m
[32m+[m[32m–í—ã–≤–µ—Å—Ç–∏ —Å–ø–∏—Å–æ–∫ –Ω–∞ —ç–∫—Ä–∞–Ω, –∫–∞–∂–¥–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ —Å –Ω–æ–≤–æ–π —Å—Ç—Ä–æ–∫–∏.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int N = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int M = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        ArrayList<String> list = new ArrayList<String>(N);[m[41m[m
[32m+[m[32m        for (int i = 0; i < N; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.add(reader.readLine());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        for (int i = 0; i < M; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.add(list.get(0));[m[41m[m
[32m+[m[32m            list.remove(0);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (int i = 0; i < list.size(); i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(list.get(i));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
