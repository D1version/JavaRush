[32m+[m[32mimport java.util.LinkedList;[m[41m[m
[32m+[m[32mimport java.util.List;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ü—Ä–æ–≤–µ—Å—Ç–∏ 10 —Ç—ã—Å—è—á –≤—Å—Ç–∞–≤–æ–∫, —É–¥–∞–ª–µ–Ω–∏–π[m[41m[m
[32m+[m[32m–î–ª—è arrayList –∏ linkedList –ø—Ä–æ–≤–µ—Å—Ç–∏ 10 —Ç—ã—Å—è—á –≤—Å—Ç–∞–≤–æ–∫, —É–¥–∞–ª–µ–Ω–∏–π, –∞ —Ç–∞–∫–∂–µ –≤—ã–∑–æ–≤–æ–≤ get –∏ set.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        // ArrayList[m[41m[m
[32m+[m[32m        ArrayList arrayList = new ArrayList();[m[41m[m
[32m+[m[32m        insert10000(arrayList);[m[41m[m
[32m+[m[32m        get10000(arrayList);[m[41m[m
[32m+[m[32m        set10000(arrayList);[m[41m[m
[32m+[m[32m        remove10000(arrayList);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // LinkedList[m[41m[m
[32m+[m[32m        LinkedList linkedList = new LinkedList();[m[41m[m
[32m+[m[32m        insert10000(linkedList);[m[41m[m
[32m+[m[32m        get10000(linkedList);[m[41m[m
[32m+[m[32m        set10000(linkedList);[m[41m[m
[32m+[m[32m        remove10000(linkedList);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void insert10000(List list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for(int i = 0; i <10000; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.add(i, "Chto");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void get10000(List list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for(int i = 0; i <10000; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.get(i);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void set10000(List list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for(int i = 0; i <10000; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.set(i, "Chtol");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void remove10000(List list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for(int i = 0; i <10000; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.remove(0);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
