[32m+[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader  = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        String s = "Max is ";[m[41m[m
[32m+[m[32m        int a = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int b = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        max = a > b ? a : b;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        System.out.println(s + max);[m[41m[m
[32m+[m[32m    }[m[41m[m
