[32m+[m[32m    {[m[41m[m
[32m+[m[32m        ArrayList<String> list = new ArrayList<String>();[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        for(int i = 0; i < 5;i++)[m[41m[m
