[32m+[m[32m        String GrandFa = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        String GrandMa = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        String fatherName = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        String motherName = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        String SonName = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        String daughterName = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        Cat cat1 = new Cat(GrandFa);[m[41m[m
[32m+[m[32m        Cat cat2 = new Cat(GrandMa);[m[41m[m
[32m+[m[32m        Cat cat3 = new Cat(fatherName, cat1, null );[m[41m[m
[32m+[m[32m        Cat cat4 = new Cat(motherName,  null, cat2);[m[41m[m
[32m+[m[32m        Cat cat5 = new Cat(SonName, cat3, cat4);[m[41m[m
[32m+[m[32m        Cat cat6 = new Cat(daughterName, cat3, cat4);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        System.out.println(cat1);[m[41m[m
[32m+[m[32m        System.out.println(cat2);[m[41m[m
[32m+[m[32m        System.out.println(cat3);[m[41m[m
[32m+[m[32m        System.out.println(cat4);[m[41m[m
[32m+[m[32m        System.out.println(cat5);[m[41m[m
[32m+[m[32m        System.out.println(cat6);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static class Cat[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        public  Cat mother;[m[41m[m
[32m+[m[32m        public  Cat father;[m[41m[m
[32m+[m[32m        public  String name;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        Cat(String name)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.name = name;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        Cat(String name, Cat father, Cat mother)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.name = name;[m[41m[m
[32m+[m[32m            this.mother = mother;[m[41m[m
[32m+[m[32m            this.father = father;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        @Override[m[41m[m
[32m+[m[32m        public String toString()[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (father == null && mother == null)[m[41m[m
[32m+[m[32m            return "Cat name is " + name + ", no mother" + ", no father" ;[m[41m[m
[32m+[m[32m            else if (mother == null)[m[41m[m
[32m+[m[32m                return "Cat name is " + name + ", no mother" + ", father is " + father.name;[m[41m[m
[32m+[m[32m            else if (father == null)[m[41m[m
[32m+[m[32m                return "Cat name is " + name + ", mother is " + mother.name + ", no father" ;[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m                return "Cat name is " + name + ", mother is " + mother.name + ", father is " + father.name;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
