[32m+[m[32m/* –ù—É–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –≤ –ø—Ä–æ–≥—Ä–∞–º–º—É –Ω–æ–≤—É—é —Ñ—É–Ω–∫—Ü–∏–æ–Ω–∞–ª—å–Ω–æ—Å—Ç—å[m[41m[m
[32m+[m[32m–ó–∞–¥–∞—á–∞: –ü—Ä–æ–≥—Ä–∞–º–º–∞ –æ–ø—Ä–µ–¥–µ–ª—è–µ—Ç, –∫–∞–∫–∞—è —Å–µ–º—å—è (—Ñ–∞–º–∏–ª–∏—é) –∂–∏–≤—ë—Ç –≤ –¥–æ–º–µ —Å —É–∫–∞–∑–∞–Ω–Ω—ã–º –Ω–æ–º–µ—Ä–æ–º.[m[41m[m
[32m+[m[32m–ù–æ–≤–∞—è –∑–∞–¥–∞—á–∞: –ü—Ä–æ–≥—Ä–∞–º–º–∞ –¥–æ–ª–∂–Ω–∞ —Ä–∞–±–æ—Ç–∞—Ç—å –Ω–µ —Å –Ω–æ–º–µ—Ä–∞–º–∏ –¥–æ–º–æ–≤, –∞ —Å –≥–æ—Ä–æ–¥–∞–º–∏:[m[41m[m
[32m+[m[32m–ü—Ä–∏–º–µ—Ä –≤–≤–æ–¥–∞:[m[41m[m
[32m+[m[32m–ú–æ—Å–∫–≤–∞[m[41m[m
[32m+[m[32m–ò–≤–∞–Ω–æ–≤—ã[m[41m[m
[32m+[m[32m–ö–∏–µ–≤[m[41m[m
[32m+[m[32m–ü–µ—Ç—Ä–æ–≤—ã[m[41m[m
[32m+[m[32m–õ–æ–Ω–¥–æ–Ω[m[41m[m
[32m+[m[32m–ê–±—Ä–∞–º–æ–≤–∏—á–∏[m[41m[m
[32m+[m[41m[m
[32m+[m[32m–õ–æ–Ω–¥–æ–Ω[m[41m[m
[32m+[m[41m[m
[32m+[m[32m–ü—Ä–∏–º–µ—Ä –≤—ã–≤–æ–¥–∞:[m[41m[m
[32m+[m[32m–ê–±—Ä–∞–º–æ–≤–∏—á–∏[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        Map<String,String> addresses = new HashMap<String, String>();[m[41m[m
[32m+[m[32m        while (true)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String city = reader.readLine();[m[41m[m
[32m+[m[32m            if(city.isEmpty()) break;[m[41m[m
[32m+[m[32m            String name = reader.readLine();[m[41m[m
[32m+[m[32m            if (!city.isEmpty() & !name.isEmpty())[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                addresses.put(city, name);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else break;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        String tempCity = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (Map.Entry<String, String> pair: addresses.entrySet())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String city = pair.getKey();[m[41m[m
[32m+[m[32m            if (tempCity.equals(city))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                String name = pair.getValue();[m[41m[m
[32m+[m[32m                System.out.println(name);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
