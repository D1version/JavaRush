[32m+[m[41m[m
[32m+[m[32mimport java.io.BufferedReader;[m[41m[m
[32m+[m[32mimport java.io.IOException;[m[41m[m
[32m+[m[32mimport java.io.InputStreamReader;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ù—É–∂–Ω–æ –∏—Å–ø—Ä–∞–≤–∏—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É, —á—Ç–æ–±—ã –∫–æ–º–ø–∏–ª–∏—Ä–æ–≤–∞–ª–∞—Å—å –∏ —Ä–∞–±–æ—Ç–∞–ª–∞[m[41m[m
[32m+[m[32m–ó–∞–¥–∞—á–∞: –ü—Ä–æ–≥—Ä–∞–º–º–∞ –≤–≤–æ–¥–∏—Ç –¥–≤–∞ –∏–º–µ–Ω–∏ —Ñ–∞–π–ª–∞. –ò –∫–æ–ø–∏—Ä—É–µ—Ç –ø–µ—Ä–≤—ã–π —Ñ–∞–π–ª –Ω–∞ –º–µ—Å—Ç–æ –∑–∞–¥–∞–Ω–Ω–æ–µ –≤—Ç–æ—Ä—ã–º –∏–º–µ–Ω–µ–º.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        String sourceFileName = reader.readLine();[m[41m[m
[32m+[m[32m        String destinationFileName = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        java.io.FileInputStream fileInputStream = new java.io.FileInputStream(sourceFileName);[m[41m[m
[32m+[m[32m        java.io.FileOutputStream fileOutputStream = new java.io.FileOutputStream(destinationFileName);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int count = 0;[m[41m[m
[32m+[m[32m        while (fileInputStream.available()>0)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int data = fileInputStream.read();[m[41m[m
[32m+[m[32m            fileOutputStream.write(data);[m[41m[m
[32m+[m[32m            count++;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        System.out.println("–°–∫–æ–ø–∏—Ä–æ–≤–∞–Ω–æ –±–∞–π—Ç " + count);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        fileInputStream.close();[m[41m[m
[32m+[m[32m        fileOutputStream.close();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
