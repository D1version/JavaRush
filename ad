[32m+[m[41m[m
[32m+[m[32m    public static double getDistance(int x1, int y1, int x2, int y2) {[m[41m[m
[32m+[m[32m        return Math.sqrt((x2-x1)*(x2-x1) + (y2-y1)*(y2-y1));[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
