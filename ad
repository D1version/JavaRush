[32m+[m[32mpackage com.javarush.test.level04.lesson16.home03;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.io.*;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ü–æ—Å—á–∏—Ç–∞—Ç—å —Å—É–º–º—É —á–∏—Å–µ–ª[m[41m[m
[32m+[m[32m–í–≤–æ–¥–∏—Ç—å —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã —á–∏—Å–ª–∞ –∏ —Å—á–∏—Ç–∞—Ç—å –∏—Ö —Å—É–º–º—É. –ï—Å–ª–∏ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å –≤–≤–µ–ª -1, –≤—ã–≤–µ—Å—Ç–∏ –Ω–∞ —ç–∫—Ä–∞–Ω —Å—É–º–º—É –∏ –∑–∞–≤–µ—Ä—à–∏—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É.  -1 –¥–æ–ª–∂–Ω–æ —É—á–∏—Ç—ã–≤–∞—Ç—å—Å—è –≤ —Å—É–º–º–µ.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args)   throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        int sum = 0;[m[41m[m
[32m+[m[32m        while (true)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int a = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m            sum = sum + a;[m[41m[m
[32m+[m[32m            if(a == -1) break;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        System.out.println(sum);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
