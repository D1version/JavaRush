[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Map<String, String> map = new HashMap<String, String>();[m[41m[m
[32m+[m[32m        map.put("–∞—Ä–±—É–∑", "—è–≥–æ–¥–∞");[m[41m[m
[32m+[m[32m        map.put("–±–∞–Ω–∞–Ω", "—Ç—Ä–∞–≤–∞");[m[41m[m
