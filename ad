[32m+[m[32m        throw new NullPointerException();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public void method2() {[m[41m[m
[32m+[m[32m        throw new IndexOutOfBoundsException();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
