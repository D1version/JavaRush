[32m+[m[32mpackage com.javarush.test.level07.lesson04.task01;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.io.BufferedReader;[m[41m[m
[32m+[m[32mimport java.io.IOException;[m[41m[m
[32m+[m[32mimport java.io.InputStreamReader;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ú–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–µ —Å—Ä–µ–¥–∏ –º–∞—Å—Å–∏–≤–∞ –Ω–∞ 20 —á–∏—Å–µ–ª[m[41m[m
[32m+[m[32m1. –í –º–µ—Ç–æ–¥–µ initializeArray():[m[41m[m
[32m+[m[32m1.1. –°–æ–∑–¥–∞–π—Ç–µ –º–∞—Å—Å–∏–≤ –Ω–∞ 20 —á–∏—Å–µ–ª[m[41m[m
[32m+[m[32m1.2. –°—á–∏—Ç–∞–π—Ç–µ —Å –∫–æ–Ω—Å–æ–ª–∏ 20 —á–∏—Å–µ–ª –∏ –∑–∞–ø–æ–ª–Ω–∏—Ç–µ –∏–º–∏ –º–∞—Å—Å–∏–≤[m[41m[m
[32m+[m[32m2. –ú–µ—Ç–æ–¥ max(int[] array) –¥–æ–ª–∂–µ–Ω –Ω–∞—Ö–æ–¥–∏—Ç—å –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–µ —á–∏—Å–ª–æ –∏–∑ —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –º–∞—Å—Å–∏–≤–∞[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        int[] array = initializeArray();[m[41m[m
[32m+[m[32m        int max = max(array);[m[41m[m
[32m+[m[32m        System.out.println(max);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    public static int[] initializeArray() throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        int[] list = new int[20];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (int i = 0; i < list.length; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String s = reader.readLine();[m[41m[m
[32m+[m[32m            list[i] = Integer.parseInt(s);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return list;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static int max(int[] array)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int max = array[0];[m[41m[m
[32m+[m[32m        for (int i = 0; i < array.length; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (array[i] > max)[m[41m[m
[32m+[m[32m                max = array[i];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return max;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m
