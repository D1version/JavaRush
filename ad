[32m+[m[32mpackage com.javarush.test.level08.lesson08.task05;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.util.HashMap;[m[41m[m
[32m+[m[32mimport java.util.Iterator;[m[41m[m
[32m+[m[32mimport java.util.Map;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –£–¥–∞–ª–∏—Ç—å –ª—é–¥–µ–π, –∏–º–µ—é—â–∏—Ö –æ–¥–∏–Ω–∞–∫–æ–≤—ã–µ –∏–º–µ–Ω–∞[m[41m[m
[32m+[m[32m–°–æ–∑–¥–∞—Ç—å —Å–ª–æ–≤–∞—Ä—å (Map<String, String>) –∑–∞–Ω–µ—Å—Ç–∏ –≤ –Ω–µ–≥–æ –¥–µ—Å—è—Ç—å –∑–∞–ø–∏—Å–µ–π –ø–æ –ø—Ä–∏–Ω—Ü–∏–ø—É ¬´—Ñ–∞–º–∏–ª–∏—è¬ª - ¬´–∏–º—è¬ª.[m[41m[m
[32m+[m[32m–£–¥–∞–ª–∏—Ç—å –ª—é–¥–µ–π, –∏–º–µ—é—â–∏—Ö –æ–¥–∏–Ω–∞–∫–æ–≤—ã–µ –∏–º–µ–Ω–∞.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static HashMap<String, String> createMap()[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashMap<String, String> map = new HashMap<String, String>();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (int i = 0; i < 9; i++)[m[41m[m
[32m+[m[32m            map.put("–§–∞–º–∏–ª–∏—è" + i, "–ò–º—è" + i);[m[41m[m
[32m+[m[32m        map.put("–§–∞–º–∏–ª–∏—è10", "–ò–º—è7");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return map;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void removeTheFirstNameDuplicates(HashMap<String, String> map)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashMap<String, String> copy = new HashMap<String, String>(map);[m[41m[m
[32m+[m[32m        HashMap<String, String> del = new HashMap<String, String>(map);[m[41m[m
[32m+[m[32m        for (Map.Entry<String, String> pair : copy.entrySet())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            del.remove(pair.getKey());[m[41m[m
[32m+[m[32m            if (del.containsValue(pair.getValue()))[m[41m[m
[32m+[m[32m                removeItemFromMapByValue(map, pair.getValue());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void removeItemFromMapByValue(HashMap<String, String> map, String value)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashMap<String, String> copy = new HashMap<String, String>(map);[m[41m[m
[32m+[m[32m        for (Map.Entry<String, String> pair : copy.entrySet())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (pair.getValue().equals(value))[m[41m[m
[32m+[m[32m                map.remove(pair.getKey());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
