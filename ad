[32m+[m[32mpackage com.javarush.test.level09.lesson11.home09;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.util.HashMap;[m[41m[m
[32m+[m[32mimport java.util.HashSet;[m[41m[m
[32m+[m[32mimport java.util.Map;[m[41m[m
[32m+[m[32mimport java.util.Set;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –î–µ—Å—è—Ç—å –∫–æ—Ç–æ–≤[m[41m[m
[32m+[m[32m–°–æ–∑–¥–∞—Ç—å –∫–ª–∞—Å—Å –∫–æ—Ç ‚Äì Cat, —Å –ø–æ–ª–µ–º ¬´–∏–º—è¬ª (String).[m[41m[m
[32m+[m[32m–°–æ–∑–¥–∞—Ç—å —Å–ª–æ–≤–∞—Ä—å Map(<String, Cat>) –∏ –¥–æ–±–∞–≤–∏—Ç—å —Ç—É–¥–∞ 10 –∫–æ—Ç–æ–≤ –≤ –≤–∏–¥–µ ¬´–ò–º—è¬ª-¬´–ö–æ—Ç¬ª.[m[41m[m
[32m+[m[32m–ü–æ–ª—É—á–∏—Ç—å –∏–∑ Map –º–Ω–æ–∂–µ—Å—Ç–≤–æ(Set) –≤—Å–µ—Ö –∏–º–µ–Ω –∏ –≤—ã–≤–µ—Å—Ç–∏ –µ–≥–æ –Ω–∞ —ç–∫—Ä–∞–Ω.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Map<String, Cat> map = createMap();[m[41m[m
[32m+[m[32m        Set<Cat> set = convertMapToSet(map);[m[41m[m
[32m+[m[32m        printCatSet(set);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static Map<String, Cat> createMap()[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Cat cat1 = new Cat("cat1");[m[41m[m
[32m+[m[32m        Cat cat2 = new Cat("cat2");[m[41m[m
[32m+[m[32m        Cat cat3 = new Cat("cat3");[m[41m[m
[32m+[m[32m        Cat cat4 = new Cat("cat4");[m[41m[m
[32m+[m[32m        Cat cat5 = new Cat("cat5");[m[41m[m
[32m+[m[32m        Cat cat6 = new Cat("cat6");[m[41m[m
[32m+[m[32m        Cat cat7 = new Cat("cat7");[m[41m[m
[32m+[m[32m        Cat cat8 = new Cat("cat8");[m[41m[m
[32m+[m[32m        Cat cat9 = new Cat("cat9");[m[41m[m
[32m+[m[32m        Cat cat10 = new Cat("cat10");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m      Map<String, Cat> list = new HashMap<String, Cat>();[m[41m[m
[32m+[m[32m        list.put("cat1", cat1);[m[41m[m
[32m+[m[32m        list.put("cat2", cat2);[m[41m[m
[32m+[m[32m        list.put("cat3", cat3);[m[41m[m
[32m+[m[32m        list.put("cat4", cat4);[m[41m[m
[32m+[m[32m        list.put("cat5", cat5);[m[41m[m
[32m+[m[32m        list.put("cat6", cat6);[m[41m[m
[32m+[m[32m        list.put("cat7", cat7);[m[41m[m
[32m+[m[32m        list.put("cat8", cat8);[m[41m[m
[32m+[m[32m        list.put("cat9", cat9);[m[41m[m
[32m+[m[32m        list.put("cat10", cat10);[m[41m[m
[32m+[m[32m        return list;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static Set<Cat> convertMapToSet(Map<String, Cat> map)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Set<Cat> set = new HashSet<Cat>();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (Map.Entry<String, Cat> list : map.entrySet())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            set.add(list.getValue());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return set;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void printCatSet(Set<Cat> set)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (Cat cat:set)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(cat);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static class Cat[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        private String name;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public Cat(String name)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.name = name;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public String toString()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return "Cat "+this.name;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
