[32m+[m[32mpackage com.javarush.test.level08.lesson03.task06;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.util.Date;[m[41m[m
[32m+[m[32mimport java.util.HashMap;[m[41m[m
[32m+[m[32mimport java.util.Map;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ö–æ–ª–ª–µ–∫—Ü–∏—è HashMap –∏–∑ Object[m[41m[m
[32m+[m[32m–ï—Å—Ç—å –∫–æ–ª–ª–µ–∫—Ü–∏—è HashMap<String, Object>, —Ç—É–¥–∞ –∑–∞–Ω–µ—Å–ª–∏ 10 —Ä–∞–∑–ª–∏—á–Ω—ã—Ö –ø–∞—Ä –æ–±—ä–µ–∫—Ç–æ–≤.[m[41m[m
[32m+[m[32m–í—ã–≤–µ—Å—Ç–∏ —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ –∫–æ–ª–ª–µ–∫—Ü–∏–∏ –Ω–∞ —ç–∫—Ä–∞–Ω, –∫–∞–∂–¥—ã–π —ç–ª–µ–º–µ–Ω—Ç —Å –Ω–æ–≤–æ–π —Å—Ç—Ä–æ–∫–∏.[m[41m[m
[32m+[m[32m–ü—Ä–∏–º–µ—Ä –≤—ã–≤–æ–¥–∞ (—Ç—É—Ç –ø–æ–∫–∞–∑–∞–Ω–∞ —Ç–æ–ª—å–∫–æ –æ–¥–Ω–∞ —Å—Ç—Ä–æ–∫–∞):[m[41m[m
[32m+[m[32mSim - 5[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Date yearStartTime = new Date();[m[41m[m
[32m+[m[32m        yearStartTime.setHours(0);[m[41m[m
[32m+[m[32m        yearStartTime.setMinutes(0);[m[41m[m
[32m+[m[32m        yearStartTime.setSeconds(0);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        yearStartTime.setDate(1);      // –ø–µ—Ä–≤–æ–µ —á–∏—Å–ª–æ[m[41m[m
[32m+[m[32m        yearStartTime.setMonth(0);     // –º–µ—Å—è—Ü —è–Ω–≤–∞—Ä—å, –Ω—É–º–µ—Ä–∞—Ü–∏—è –¥–ª—è –º–µ—Å—è—Ü–µ–≤ 0-11[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        Date currentTime = new Date();[m[41m[m
[32m+[m[32m        long msTimeDistance = currentTime.getTime() - yearStartTime.getTime();[m[41m[m
[32m+[m[32m        long msDay = 24 * 60 * 60 * 1000;  //—Å–∫–æ–ª—å–∫–æ –º–∏–ª–ª–∏—Å–µ–∫—É–Ω–¥ –≤ –æ–¥–Ω–∏—Ö —Å—É—Ç–∫–∞—Ö[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int dayCount = (int) (msTimeDistance/msDay); //–∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ü–µ–ª—ã—Ö –¥–Ω–µ–π[m[41m[m
[32m+[m[32m        System.out.println("Days from start of year: " + dayCount);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
