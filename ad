[32m+[m[41m[m
[32m+[m[32mimport java.io.BufferedReader;[m[41m[m
[32m+[m[32mimport java.io.InputStreamReader;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ó–∞–¥–∞—á–∞ –ø–æ –∞–ª–≥–æ—Ä–∏—Ç–º–∞–º[m[41m[m
[32m+[m[32m–ó–∞–¥–∞—á–∞: –ù–∞–ø–∏—Å–∞—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É, –∫–æ—Ç–æ—Ä–∞—è –≤–≤–æ–¥–∏—Ç —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã 20 —á–∏—Å–µ–ª –∏ –≤—ã–≤–æ–¥–∏—Ç –∏—Ö –≤ —É–±—ã–≤–∞—é—â–µ–º –ø–æ—Ä—è–¥–∫–µ.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        int[] array = new int[20];[m[41m[m
[32m+[m[32m        for (int i = 0; i < 20; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            array[i] = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        sort(array);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (int x : array)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(x);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void sort(int[] array)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        //–ù–∞–ø–∏—à–∏—Ç–µ —Ç—É—Ç –≤–∞—à –∫–æ–¥[m[41m[m
[32m+[m[32m        for (int i = 0; i < array.length; i++) {[m[41m[m
[32m+[m[32m            for (int j = 0; j < array.length-1; j++) {[m[41m[m
[32m+[m[32m                int insert;[m[41m[m
[32m+[m[32m                if(array[j+1] > array[j]){[m[41m[m
[32m+[m[32m                    insert = array[j+1];[m[41m[m
[32m+[m[32m                    array[j+1] = array[j];[m[41m[m
[32m+[m[32m                    array[j] = insert;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
