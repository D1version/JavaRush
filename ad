[32m+[m[41m[m
[32m+[m[32mimport java.util.Calendar;[m[41m[m
[32m+[m[32mimport java.util.Date;[m[41m[m
[32m+[m[32mimport java.util.GregorianCalendar;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –†–∞–±–æ—Ç–∞ —Å –¥–∞—Ç–æ–π[m[41m[m
[32m+[m[32m1. –†–µ–∞–ª–∏–∑–æ–≤–∞—Ç—å –º–µ—Ç–æ–¥ isDateOdd(String date) —Ç–∞–∫, —á—Ç–æ–±—ã –æ–Ω –≤–æ–∑–≤—Ä–∞—â–∞–ª true, –µ—Å–ª–∏ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –¥–Ω–µ–π —Å –Ω–∞—á–∞–ª–∞ –≥–æ–¥–∞ - –Ω–µ—á–µ—Ç–Ω–æ–µ —á–∏—Å–ª–æ, –∏–Ω–∞—á–µ false[m[41m[m
[32m+[m[32m2. String date –ø–µ—Ä–µ–¥–∞–µ—Ç—Å—è –≤ —Ñ–æ—Ä–º–∞—Ç–µ MAY 1 2013[m[41m[m
[32m+[m[32m–ü—Ä–∏–º–µ—Ä:[m[41m[m
[32m+[m[32mJANUARY 1 2000 = true[m[41m[m
[32m+[m[32mJANUARY 2 2020 = false[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        System.out.println(isDateOdd("MAY 1 2013"));[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static boolean isDateOdd(String date)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        String[] temp = date.split(" ");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        Date before = new Date(date);[m[41m[m
[32m+[m[32m        Date after = new Date("JANUARY 0 " + date.split(" ")[2]);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        long difference = before.getTime() - after.getTime();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int day = (int) (difference / (24 * 60 * 60 * 1000));[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (day % 2 == 0)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return false;[m[41m[m
[32m+[m[32m        } else return true;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
