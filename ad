[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        import java.io.BufferedReader;[m[41m[m
[32m+[m[32m        import java.io.InputStreamReader;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int sum = 0;[m[41m[m
[32m+[m[32m        int a;[m[41m[m
[32m+[m[32m        for(boolean isExit = false;!isExit;)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            String aa = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (aa.equals("—Å—É–º–º–∞"))[m[41m[m
[32m+[m[32m                break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                a = Integer.parseInt(aa);[m[41m[m
[32m+[m[32m            sum = sum + a;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        System.out.println(sum);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
