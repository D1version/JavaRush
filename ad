[32m+[m[32m            System.out.print(s + " is 4 month");[m[41m[m
[32m+[m[32m        else if(s.equals("May"))[m[41m[m
[32m+[m[32m            System.out.print(s + " is 5 month");[m[41m[m
[32m+[m[32m        else if(s.equals("June"))[m[41m[m
[32m+[m[32m            System.out.print(s + " is 6 month");[m[41m[m
[32m+[m[32m        else if(s.equals("July"))[m[41m[m
[32m+[m[32m            System.out.print(s + " is 7 month");[m[41m[m
[32m+[m[32m        else if(s.equals("August"))[m[41m[m
[32m+[m[32m            System.out.print(s + " is 8 month");[m[41m[m
[32m+[m[32m        else if(s.equals("September"))[m[41m[m
[32m+[m[32m            System.out.print(s + " is 9 month");[m[41m[m
[32m+[m[32m        else if(s.equals("October"))[m[41m[m
[32m+[m[32m            System.out.print(s + " is 10 month");[m[41m[m
[32m+[m[32m        else if(s.equals("November"))[m[41m[m
[32m+[m[32m            System.out.print(s + " is 11 month");[m[41m[m
