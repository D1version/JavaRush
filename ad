[32m+[m[32m/* –ò–∑–º–µ—Ä–∏—Ç—å —Å–∫–æ–ª—å–∫–æ –≤—Ä–µ–º–µ–Ω–∏ –∑–∞–Ω–∏–º–∞–µ—Ç 10 —Ç—ã—Å—è—á –≤—ã–∑–æ–≤–æ–≤ get –¥–ª—è –∫–∞–∂–¥–æ–≥–æ —Å–ø–∏—Å–∫–∞[m[41m[m
[32m+[m[32m–ò–∑–º–µ—Ä–∏—Ç—å, —Å–∫–æ–ª—å–∫–æ –≤—Ä–µ–º–µ–Ω–∏ –∑–∞–Ω–∏–º–∞–µ—Ç 10 —Ç—ã—Å—è—á –≤—ã–∑–æ–≤–æ–≤ get –¥–ª—è –∫–∞–∂–¥–æ–≥–æ —Å–ø–∏—Å–∫–∞.[m[41m[m
[32m+[m[32m–ú–µ—Ç–æ–¥ getTimeMsOfGet  –¥–æ–ª–∂–µ–Ω –≤–µ—Ä–Ω—É—Ç—å –≤—Ä–µ–º—è –µ–≥–æ –∏—Å–ø–æ–ª–Ω–µ–Ω–∏—è –≤ –º–∏–ª–ª–∏—Å–µ–∫—É–Ω–¥–∞—Ö.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        System.out.println(getTimeMsOfGet(fill(new ArrayList())));[m[41m[m
[32m+[m[32m        System.out.println(getTimeMsOfGet(fill(new LinkedList())));[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private static List fill(List list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for(int i = 0; i < 11; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.add(new Object());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return list;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static long  getTimeMsOfGet(List list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Date start = new Date();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        get10000(list);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        Date finish = new Date();[m[41m[m
[32m+[m[32m        long msDelay = finish.getTime() - start.getTime();[m[41m[m
[32m+[m[32m        System.out.print(msDelay);[m[41m[m
[32m+[m[32m        return msDelay;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void get10000(List list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        if (list.isEmpty()) return;[m[41m[m
[32m+[m[32m        int x = list.size() / 2;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (int i = 0; i < 10000; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.get(x);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
