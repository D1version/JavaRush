[32m+[m[32mpackage com.javarush.test.level08.lesson11.home08;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.io.BufferedReader;[m[41m[m
[32m+[m[32mimport java.io.InputStreamReader;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ü—è—Ç—å –Ω–∞–∏–±–æ–ª—å—à–∏—Ö —á–∏—Å–µ–ª[m[41m[m
[32m+[m[32m–°–æ–∑–¥–∞—Ç—å –º–∞—Å—Å–∏–≤ –Ω–∞ 20 —á–∏—Å–µ–ª. –ó–∞–ø–æ–ª–Ω–∏—Ç—å –µ–≥–æ —á–∏—Å–ª–∞–º–∏ —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã. –í—ã–≤–µ—Å—Ç–∏ –ø—è—Ç—å –Ω–∞–∏–±–æ–ª—å—à–∏—Ö —á–∏—Å–µ–ª.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        int[] array = new int[20];[m[41m[m
[32m+[m[32m        for (int i = 0; i < array.length; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            array[i] = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        sort(array);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        System.out.println(array[0]);[m[41m[m
[32m+[m[32m        System.out.println(array[1]);[m[41m[m
[32m+[m[32m        System.out.println(array[2]);[m[41m[m
[32m+[m[32m        System.out.println(array[3]);[m[41m[m
[32m+[m[32m        System.out.println(array[4]);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void sort(int[] array)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (int i = 1; i<array.length; i++)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        int key = array[i];[m[41m[m
[32m+[m[32m        int j = i-1;[m[41m[m
[32m+[m[32m        while (j >=0 && array[j] < key)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            array[j + 1] = array[j];[m[41m[m
[32m+[m[32m            array[j] = key;[m[41m[m
[32m+[m[32m            j--;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
