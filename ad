[32m+[m[41m[m
[32m+[m[32m        for (Map.Entry<String, String> pair : map.entrySet())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String key = pair.getKey();[m[41m[m
[32m+[m[32m            String value = pair.getValue();[m[41m[m
[32m+[m[32m            System.out.println(key + " - " + value);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
