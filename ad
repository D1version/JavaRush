[32m+[m[32m            text += ", –≤–æ–∑—Ä–∞—Å—Ç: " + this.age;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int childCount = this.children.size();[m[41m[m
[32m+[m[32m            if (childCount > 0)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                text += ", –¥–µ—Ç–∏: "+this.children.get(0).name;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                for (int i = 1; i < childCount; i++)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    Human child = this.children.get(i);[m[41m[m
