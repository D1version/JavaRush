[32m+[m[32m–£–¥–∞–ª–∏—Ç—å –∏–∑ —Å–ª–æ–≤–∞—Ä—è –≤—Å–µ—Ö –ª—é–¥–µ–π, —Ä–æ–¥–∏–≤—à–∏—Ö—Å—è –ª–µ—Ç–æ–º.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static HashMap<String, Date> createMap()[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashMap<String, Date> map = new HashMap<String, Date>();[m[41m[m
[32m+[m[32m        map.put("Stallone", new Date("JUNE 1 1980"));[m[41m[m
[32m+[m[32m        map.put("Stallonee", new Date("OCTOBER 1 1980"));[m[41m[m
[32m+[m[32m        map.put("Stalloneee", new Date("MAY 1 1980"));[m[41m[m
[32m+[m[32m        map.put("Stallone33", new Date("AUGUST 1 1980"));[m[41m[m
[32m+[m[32m        map.put("Stallone3", new Date("JULY 1 1980"));[m[41m[m
[32m+[m[32m        map.put("Stallone2", new Date("DECEMBER 1 1980"));[m[41m[m
[32m+[m[32m        map.put("Stallone1", new Date("NOVEMBER 1 1980"));[m[41m[m
[32m+[m[32m        map.put("Stallone11", new Date("AUGUST 1 1980"));[m[41m[m
[32m+[m[32m        map.put("Stallone111", new Date("JUNE 1 1980"));[m[41m[m
[32m+[m[32m        map.put("Stallon", new Date("SEPTEMBER 1 1980"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return map;[m[41m[m
[32m+[m[32m        //–Ω–∞–ø–∏—à–∏—Ç–µ —Ç—É—Ç –≤–∞—à –∫–æ–¥[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void removeAllSummerPeople(HashMap<String, Date> map)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for(Iterator<Map.Entry<String, Date>> it = map.entrySet().iterator(); it.hasNext(); ) {[m[41m[m
[32m+[m[32m            Map.Entry<String, Date> pair = it.next();[m[41m[m
[32m+[m[32m            if(pair.getValue().getMonth() == 5 || pair.getValue().getMonth() == 6 || pair.getValue().getMonth() == 7) {[m[41m[m
[32m+[m[32m                it.remove();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
