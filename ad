[32m+[m[32m            String s = reader.readLine();[m[41m[m
[32m+[m[32m            list.add(s);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        for(int i = 0; i < 13; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String s1 = list.remove(list.size() - 1);[m[41m[m
[32m+[m[32m            list.add(0, s1);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        for(int i = 0; i < list.size(); i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(list.get(i));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
