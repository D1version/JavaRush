[32m+[m[32mpackage com.javarush.test.level07.lesson12.bonus01;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.io.BufferedReader;[m[41m[m
[32m+[m[32mimport java.io.IOException;[m[41m[m
[32m+[m[32mimport java.io.InputStreamReader;[m[41m[m
[32m+[m[32mimport java.util.ArrayList;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/*[m[41m[m
[32m+[m[32m–ó–∞–¥–∞—á–∞:  –ü—Ä–æ–≥—Ä–∞–º–º–∞ –≤–≤–æ–¥–∏—Ç —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã –¥–∞–Ω–Ω—ã–µ –ø—Ä–æ –∫–æ—Ç–æ–≤ –∏ –≤—ã–≤–æ–¥–∏—Ç –∏—Ö –Ω–∞ —ç–∫—Ä–∞–Ω. –ü—Ä–∏–º–µ—Ä:[m[41m[m
[32m+[m[32mCat name is Barsik, age is 6, weight is 5, tail = 22[m[41m[m
[32m+[m[32mCat name is Murka, age is 8, weight is 7, tail = 20[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public final static ArrayList<Cat> CATS = new ArrayList<Cat>();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        while (true)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String name = reader.readLine();[m[41m[m
[32m+[m[32m            if (name.isEmpty()) break;[m[41m[m
[32m+[m[32m            int age = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m            int weight = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m            int tail = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            Cat cat = new Cat(name, age, weight, tail);[m[41m[m
[32m+[m[32m            CATS.add(cat);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        printList();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void printList() {[m[41m[m
[32m+[m[32m        for(int i = 0; i < CATS.size(); i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(CATS.get(i));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static class Cat[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        private String name;[m[41m[m
[32m+[m[32m        private int age;[m[41m[m
[32m+[m[32m        private int weight;[m[41m[m
[32m+[m[32m        private int tailLength;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        Cat(String name, int age, int weight, int tailLength)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.name = name;[m[41m[m
[32m+[m[32m            this.age = age;[m[41m[m
[32m+[m[32m            this.weight = weight;[m[41m[m
[32m+[m[32m            this.tailLength = tailLength;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        @Override[m[41m[m
[32m+[m[32m        public String toString()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return "Cat name is " + name + ", age is " + age + ", weight is " + weight + ", tail = " + tailLength;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
