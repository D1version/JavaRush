[32m+[m[32m    public static Grandmother grandmother = new Grandmother();[m[41m[m
[32m+[m[32m    public static Patty patty = new Patty();[m[41m[m
[32m+[m[32m    public static Woodman woodman = new Woodman();[m[41m[m
[32m+[m[32m    public static Wolf wolf = new Wolf();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void main(String[] args) {[m[41m[m
[32m+[m[32m        wolf.ate.add(grandmother);[m[41m[m
[32m+[m[32m        wolf.ate.add(hood);[m[41m[m
[32m+[m[32m        woodman.killed.add(wolf);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    //–∫—Ä–∞—Å–Ω–∞—è —à–∞–ø–æ—á–∫–∞[m[41m[m
[32m+[m[32m    public static class LittleRedRidingHood extends StoryItem {[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    //–±–∞–±—É—à–∫–∞[m[41m[m
[32m+[m[32m    public static class Grandmother extends StoryItem {[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    //–ø–∏—Ä–æ–∂–æ–∫[m[41m[m
[32m+[m[32m    public static class Patty extends StoryItem {[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    //–¥—Ä–æ–≤–æ—Å–µ–∫[m[41m[m
[32m+[m[32m    public static class Woodman extends StoryItem {[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    //–≤–æ–ª–∫[m[41m[m
[32m+[m[32m    public static class Wolf extends StoryItem {[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
