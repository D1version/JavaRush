[32m+[m[32mimport java.util.ArrayList;[m[41m[m
[32m+[m[32m/* –£–¥–≤–æ–π —Å–ª–æ–≤–∞[m[41m[m
[32m+[m[32m1. –í–≤–µ–¥–∏ —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã 10 —Å–ª–æ–≤ –≤ —Å–ø–∏—Å–æ–∫ —Å—Ç—Ä–æ–∫.[m[41m[m
[32m+[m[32m2. –ú–µ—Ç–æ–¥ doubleValues –¥–æ–ª–∂–µ–Ω —É–¥–≤–∞–∏–≤–∞—Ç—å —Å–ª–æ–≤–∞ –ø–æ –ø—Ä–∏–Ω—Ü–∏–ø—É a,b,c -> a,a,b,b,c,c.[m[41m[m
[32m+[m[32m3. –ò—Å–ø–æ–ª—å–∑—É—è —Ü–∏–∫–ª for –≤—ã–≤–µ–¥–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç –Ω–∞ —ç–∫—Ä–∞–Ω, –∫–∞–∂–¥–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ —Å –Ω–æ–≤–æ–π —Å—Ç—Ä–æ–∫–∏.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        ArrayList<String>list = new ArrayList<String>();[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        for (int i = 0; i < 10; i++)[m[41m[m
[32m+[m[32m            list.add(reader.readLine());[m[41m[m
[32m+[m[32m        ArrayList<String> result = doubleValues(list);[m[41m[m
[32m+[m[32m        for (int i = 0; i < result.size(); i++)[m[41m[m
[32m+[m[32m            System.out.println(result.get(i));[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    public static ArrayList<String> doubleValues(ArrayList<String> list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (int i = 0; i < list.size(); i = i + 2)[m[41m[m
[32m+[m[32m            list.add(i, list.get(i));[m[41m[m
[32m+[m[32m        return list;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
