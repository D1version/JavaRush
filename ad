[32m+[m[32m/* –ü–µ—Ä–µ—Ö–≤–∞—Ç –≤—ã–±–æ—Ä–æ—á–Ω—ã—Ö –∏—Å–∫–ª—é—á–µ–Ω–∏–π[m[41m[m
[32m+[m[32m1. –†–∞–∑–±–µ—Ä–∏—Å—å, –∫–∞–∫–∏–µ –∏—Å–∫–ª—é—á–µ–Ω–∏—è –±—Ä–æ—Å–∞–µ—Ç –º–µ—Ç–æ–¥ BEAN.methodThrowExceptions.[m[41m[m
[32m+[m[32m2. –ú–µ—Ç–æ–¥ processExceptions –¥–æ–ª–∂–µ–Ω –≤—ã–∑—ã–≤–∞—Ç—å –º–µ—Ç–æ–¥ BEAN.methodThrowExceptions –∏ –æ–±—Ä–∞–±–∞—Ç—ã–≤–∞—Ç—å –∏—Å–∫–ª—é—á–µ–Ω–∏—è:[m[41m[m
[32m+[m[32m2.1. –µ—Å–ª–∏ –≤–æ–∑–Ω–∏–∫–ª–æ –∏—Å–∫–ª—é—á–µ–Ω–∏–µ FileSystemException, —Ç–æ –ª–æ–≥–∏—Ä–æ–≤–∞—Ç—å –µ–≥–æ (–≤—ã–∑–≤–∞—Ç—å –º–µ—Ç–æ–¥ BEAN.log) –∏ –ø—Ä–æ–±—Ä–æ—Å–∏—Ç—å –¥–∞–ª—å—à–µ[m[41m[m
[32m+[m[32m2.2. –µ—Å–ª–∏ –≤–æ–∑–Ω–∏–∫–ª–æ –∏—Å–∫–ª—é—á–µ–Ω–∏–µ CharConversionException –∏–ª–∏ –ª—é–±–æ–µ –¥—Ä—É–≥–æ–µ IOException, —Ç–æ —Ç–æ–ª—å–∫–æ –ª–æ–≥–∏—Ä–æ–≤–∞—Ç—å –µ–≥–æ - –≤—ã–∑–≤–∞—Ç—å –º–µ—Ç–æ–¥ BEAN.log[m[41m[m
[32m+[m[32m3. –î–æ–±–∞–≤—å –≤ —Å–∏–≥–Ω–∞—Ç—É—Ä—É –º–µ—Ç–æ–¥–∞ processExceptions –∫–ª–∞—Å—Å –∏—Å–∫–ª—é—á–µ–Ω–∏—è, –∫–æ—Ç–æ—Ä–æ–µ —Ç—ã –ø—Ä–æ–±—Ä–∞—Å—ã–≤–∞–µ—à—å –≤ –ø.2.1.[m[41m[m
[32m+[m[32m4. –í –º–µ—Ç–æ–¥–µ main –æ–±—Ä–∞–±–æ—Ç–∞–π –æ—Å—Ç–∞–≤—à–µ–µ—Å—è –∏—Å–∫–ª—é—á–µ–Ω–∏–µ - –ª–æ–≥–∏—Ä—É–π –µ–≥–æ. –ò—Å–ø–æ–ª—å–∑—É–π try..catch[m[41m[m
[32m+[m[41m[m
[32m+[m[32m–ü–æ–¥—Å–∫–∞–∑–∫–∞:[m[41m[m
[32m+[m[32m–ï—Å–ª–∏ —Ç—ã –∑–∞—Ö–≤–∞—Ç–∏–ª –∏—Å–∫–ª—é—á–µ–Ω–∏–µ MyException, –∫–æ—Ç–æ—Ä–æ–µ –Ω–µ —Ö–æ—Ç–µ–ª –∑–∞—Ö–≤–∞—Ç—ã–≤–∞—Ç—å, –µ–≥–æ –º–æ–∂–Ω–æ –ø—Ä–æ–±—Ä–æ—Å–∏—Ç—å –¥–∞–ª—å—à–µ –∫–æ–¥–æ–º –≤–∏–¥–∞:[m[41m[m
[32m+[m[32mcatch (MyException e) {[m[41m[m
[32m+[m[32mthrow e;[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution {[m[41m[m
[32m+[m[32m    public static StatelessBean BEAN = new StatelessBean();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void main(String[] args)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        try[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            processExceptions();[m[41m[m
[32m+[m[32m        }catch (Exception e){[m[41m[m
[32m+[m[32m            BEAN.log(e);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void processExceptions() throws FileSystemException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        try{[m[41m[m
[32m+[m[32m            BEAN.methodThrowExceptions();[m[41m[m
[32m+[m[32m        }catch (FileSystemException e){[m[41m[m
[32m+[m[32m            BEAN.log(e);[m[41m[m
[32m+[m[32m            throw e;[m[41m[m
[32m+[m[32m        }catch (CharConversionException e){[m[41m[m
[32m+[m[32m            BEAN.log(e);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        catch (IOException e){[m[41m[m
[32m+[m[32m            BEAN.log(e);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static class StatelessBean {[m[41m[m
[32m+[m[32m        public void log(Exception exception) {[m[41m[m
[32m+[m[32m            System.out.println(exception.getMessage() + ", " + exception.getClass().getSimpleName());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public void methodThrowExceptions() throws CharConversionException, FileSystemException, IOException {[m[41m[m
[32m+[m[32m            int i = (int) (Math.random() * 3);[m[41m[m
[32m+[m[32m            if (i == 0)[m[41m[m
[32m+[m[32m                throw new CharConversionException();[m[41m[m
[32m+[m[32m            if (i == 1)[m[41m[m
[32m+[m[32m                throw new FileSystemException("");[m[41m[m
[32m+[m[32m            if (i == 2)[m[41m[m
[32m+[m[32m                throw new IOException();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
