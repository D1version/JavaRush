[32m+[m[32mpackage com.javarush.test.level09.lesson11.home08;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.util.ArrayList;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –°–ø–∏—Å–æ–∫ –∏–∑ –º–∞—Å—Å–∏–≤–æ–≤ —á–∏—Å–µ–ª[m[41m[m
[32m+[m[32m–°–æ–∑–¥–∞—Ç—å —Å–ø–∏—Å–æ–∫, —ç–ª–µ–º–µ–Ω—Ç–∞–º–∏ –∫–æ—Ç–æ—Ä–æ–≥–æ –±—É–¥—É—Ç –º–∞—Å—Å–∏–≤—ã —á–∏—Å–µ–ª. –î–æ–±–∞–≤–∏—Ç—å –≤ —Å–ø–∏—Å–æ–∫ –ø—è—Ç—å –æ–±—ä–µ–∫—Ç–æ–≤‚Äì–º–∞—Å—Å–∏–≤–æ–≤ –¥–ª–∏–Ω–æ–π 5, 2, 4, 7, 0 —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ. –ó–∞–ø–æ–ª–Ω–∏—Ç—å –º–∞—Å—Å–∏–≤—ã –ª—é–±—ã–º–∏ –¥–∞–Ω–Ω—ã–º–∏ –∏ –≤—ã–≤–µ—Å—Ç–∏ –∏—Ö –Ω–∞ —ç–∫—Ä–∞–Ω.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        ArrayList<int[]> list = createList();[m[41m[m
[32m+[m[32m        printList(list);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static ArrayList<int[]> createList()[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        int[] array1 = new int[5];[m[41m[m
[32m+[m[32m        int[] array2 = new int[2];[m[41m[m
[32m+[m[32m        int[] array3 = new int[4];[m[41m[m
[32m+[m[32m        int[] array4 = new int[7];[m[41m[m
[32m+[m[32m        int[] array5 = new int[0];[m[41m[m
[32m+[m[32m        ArrayList<int[]> createdList = new ArrayList<int[]>();[m[41m[m
[32m+[m[32m        createdList.add(array1);[m[41m[m
[32m+[m[32m        createdList.add(array2);[m[41m[m
[32m+[m[32m        createdList.add(array3);[m[41m[m
[32m+[m[32m        createdList.add(array4);[m[41m[m
[32m+[m[32m        createdList.add(array5);[m[41m[m
[32m+[m[32m        for (int i = 0; i < createdList.size(); i++) {[m[41m[m
[32m+[m[32m            for (int j = 0; j < createdList.get(i).length; j++) {[m[41m[m
[32m+[m[32m                createdList.get(i)[j] = j;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return createdList;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void printList(ArrayList<int[]> list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (int[] array: list )[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            for (int x: array)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                System.out.println(x);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
