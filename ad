[32m+[m[32m/* –ë—É–∫–≤–∞ ¬´—Ä¬ª –∏ –±—É–∫–≤–∞ ¬´–ª¬ª[m[41m[m
[32m+[m[32m1. –°–æ–∑–¥–∞–π —Å–ø–∏—Å–æ–∫ —Å–ª–æ–≤, –∑–∞–ø–æ–ª–Ω–∏ –µ–≥–æ —Å–∞–º–æ—Å—Ç–æ—è—Ç–µ–ª—å–Ω–æ.[m[41m[m
[32m+[m[32m2. –ú–µ—Ç–æ–¥ fix –¥–æ–ª–∂–µ–Ω:[m[41m[m
[32m+[m[32m2.1. —É–¥–∞–ª—è—Ç—å –∏–∑ —Å–ø–∏—Å–∫–∞ —Å—Ç—Ä–æ–∫ –≤—Å–µ —Å–ª–æ–≤–∞, —Å–æ–¥–µ—Ä–∂–∞—â–∏–µ –±—É–∫–≤—É ¬´—Ä¬ª[m[41m[m
[32m+[m[32m2.2. —É–¥–≤–∞–∏–≤–∞—Ç—å –≤—Å–µ —Å–ª–æ–≤–∞ —Å–æ–¥–µ—Ä–∂–∞—â–∏–µ –±—É–∫–≤—É ¬´–ª¬ª.[m[41m[m
[32m+[m[32m2.3. –µ—Å–ª–∏ —Å–ª–æ–≤–æ —Å–æ–¥–µ—Ä–∂–∏—Ç –∏ –±—É–∫–≤—É ¬´—Ä¬ª –∏ –±—É–∫–≤—É ¬´–ª¬ª, —Ç–æ –æ—Å—Ç–∞–≤–∏—Ç—å —ç—Ç–æ —Å–ª–æ–≤–æ –±–µ–∑ –∏–∑–º–µ–Ω–µ–Ω–∏–π.[m[41m[m
[32m+[m[32m2.4. —Å –¥—Ä—É–≥–∏–º–∏ —Å–ª–æ–≤–∞–º–∏ –Ω–∏—á–µ–≥–æ –Ω–µ –¥–µ–ª–∞—Ç—å.[m[41m[m
[32m+[m[32m–ü—Ä–∏–º–µ—Ä:[m[41m[m
[32m+[m[32m—Ä–æ–∑–∞[m[41m[m
[32m+[m[32m–ª–∏—Ä–∞[m[41m[m
[32m+[m[32m–ª–æ–∑–∞[m[41m[m
[32m+[m[32m–í—ã—Ö–æ–¥–Ω—ã–µ –¥–∞–Ω–Ω—ã–µ:[m[41m[m
[32m+[m[32m–ª–∏—Ä–∞[m[41m[m
[32m+[m[32m–ª–æ–∑–∞[m[41m[m
[32m+[m[32m–ª–æ–∑–∞[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ArrayList<String> list = new ArrayList<String>();[m[41m[m
[32m+[m[32m        list.add("—Ä–æ–∑–∞"); //0[m[41m[m
[32m+[m[32m        list.add("–ª–∏—Ä–∞"); //1[m[41m[m
[32m+[m[32m        list.add("–ª–æ–∑–∞"); //2[m[41m[m
[32m+[m[32m        list = fix(list);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (String s : list)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(s);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static ArrayList<String> fix(ArrayList<String> list)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (int i = 0; i < list.size(); )[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if ((list.get(i).contains("–ª")) && !(list.get(i).contains("—Ä")))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                list.add(i, list.get(i));[m[41m[m
[32m+[m[32m                i = i + 2;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            else i++;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        for (int i = 0; i < list.size();)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if ((list.get(i).contains("—Ä")) && !(list.get(i).contains("–ª"))) {[m[41m[m
[32m+[m[32m                list.remove(i);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else i++;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return list;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m
