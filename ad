[32m+[m[32m        System.out.println(–ö–æ–ª—è);[m[41m[m
[32m+[m[32m        System.out.println(–í–∏–∫–∞);[m[41m[m
[32m+[m[32m        System.out.println(–ê–Ω—è);[m[41m[m
[32m+[m[32m        System.out.println(–î–µ–Ω–∏—Å);[m[41m[m
[32m+[m[32m        System.out.println(–ê–ª—ë–Ω–∞);[m[41m[m
[32m+[m[32m        System.out.println(–í–∞–¥–∏–º);[m[41m[m
[32m+[m[32m        System.out.println(–ê–ª—ë—à–∞);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static class Human[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        private final String name;[m[41m[m
[32m+[m[32m        private final boolean sex;[m[41m[m
[32m+[m[32m        private final int age;[m[41m[m
[32m+[m[32m        private final Human father;[m[41m[m
[32m+[m[32m        private final Human mother;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private Human(String name, boolean sex, int age, Human father, Human mother)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.name = name;[m[41m[m
[32m+[m[32m            this.sex = sex;[m[41m[m
[32m+[m[32m            this.age = age;[m[41m[m
[32m+[m[32m            this.father = father;[m[41m[m
[32m+[m[32m            this.mother = mother;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public String toString()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String text = "";[m[41m[m
[32m+[m[32m            text += "–ò–º—è: " + this.name;[m[41m[m
[32m+[m[32m            text += ", –ø–æ–ª: " + (this.sex ? "–º—É–∂—Å–∫–æ–π" : "–∂–µ–Ω—Å–∫–∏–π");[m[41m[m
[32m+[m[32m            text += ", –≤–æ–∑—Ä–∞—Å—Ç: " + this.age;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (this.father != null)[m[41m[m
[32m+[m[32m                text += ", –æ—Ç–µ—Ü: " + this.father.name;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (this.mother != null)[m[41m[m
[32m+[m[32m                text += ", –º–∞—Ç—å: " + this.mother.name;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return text;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
