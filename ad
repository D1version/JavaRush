[32m+[m[32m3. –í –º–µ—Ç–æ–¥–µ main —É–¥–∞–ª–∏—Ç–µ –æ–¥–Ω–æ–≥–æ –∫–æ—Ç–∞ –∏–∑ Set cats.[m[41m[m
[32m+[m[32m4. –†–µ–∞–ª–∏–∑–æ–≤–∞—Ç—å –º–µ—Ç–æ–¥ printCats, –æ–Ω –¥–æ–ª–∂–µ–Ω –≤—ã–≤–µ—Å—Ç–∏ –Ω–∞ —ç–∫—Ä–∞–Ω –≤—Å–µ—Ö –∫–æ—Ç–æ–≤, –∫–æ—Ç–æ—Ä—ã–µ –æ—Å—Ç–∞–ª–∏—Å—å –≤–æ –º–Ω–æ–∂–µ—Å—Ç–≤–µ. –ö–∞–∂–¥—ã–π –∫–æ—Ç —Å –Ω–æ–≤–æ–π —Å—Ç—Ä–æ–∫–∏.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static class Cat[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m       static Cat Cat1 = new Cat();[m[41m[m
[32m+[m[32m       static Cat Cat2 = new Cat();[m[41m[m
[32m+[m[32m        static Cat Cat3 = new Cat();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void main(String[] args)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Set<Cat> cats = createCats();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        cats.remove(Cat.Cat3);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        printCats(cats);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static Set<Cat> createCats()[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Set<Cat> cats = new HashSet<Cat>();[m[41m[m
[32m+[m[32m        cats.add(Cat.Cat1);[m[41m[m
[32m+[m[32m        cats.add(Cat.Cat2);[m[41m[m
[32m+[m[32m        cats.add(Cat.Cat3);[m[41m[m
[32m+[m[32m        return cats;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void printCats(Set<Cat> cats)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Iterator<Cat> iterator = cats.iterator();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        while (iterator.hasNext())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            Cat text = iterator.next();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            System.out.println(text);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
