[32m+[m[32m15[m[41m[m
[32m+[m[32m17[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        int a = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int b = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int c = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int d = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int e = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int buffer;[m[41m[m
[32m+[m[32m        for (int i = 0; i < 5; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (a > b)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                buffer = b;[m[41m[m
[32m+[m[32m                b = a;[m[41m[m
[32m+[m[32m                a = buffer;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (b > c)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                buffer = c;[m[41m[m
[32m+[m[32m                c = b;[m[41m[m
[32m+[m[32m                b = buffer;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (c > d)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                buffer = d;[m[41m[m
[32m+[m[32m                d = c;[m[41m[m
[32m+[m[32m                c = buffer;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (d > e)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                buffer = e;[m[41m[m
[32m+[m[32m                e = d;[m[41m[m
[32m+[m[32m                d = buffer;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        System.out.println(a);[m[41m[m
[32m+[m[32m        System.out.println(b);[m[41m[m
[32m+[m[32m        System.out.println(c);[m[41m[m
[32m+[m[32m        System.out.println(d);[m[41m[m
[32m+[m[32m        System.out.println(e);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
