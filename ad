[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int maximum = Integer.MIN_VALUE;[m[41m[m
[32m+[m[32m        int minimum = Integer.MAX_VALUE;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int[] massiv = new int[20];[m[41m[m
[32m+[m[32m        for (int i = 0; i < 20; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            massiv[i]=Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        for (int i = 0; i < massiv.length; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (massiv[i] < minimum) minimum = massiv[i];[m[41m[m
[32m+[m[32m            else if (massiv[i] > maximum) maximum = massiv[i];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        System.out.println(maximum + " " + minimum);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
