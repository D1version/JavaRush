[32m+[m[32m–Ø–±–ª–æ–∫–æ[m[41m[m
[32m+[m[32m2[m[41m[m
[32m+[m[32m0[m[41m[m
[32m+[m[32m–ê—Ä–±—É–∑[m[41m[m
[32m+[m[32m–ü—Ä–∏–º–µ—Ä –≤—ã–≤–æ–¥–∞:[m[41m[m
[32m+[m[32m–ê—Ä–±—É–∑[m[41m[m
[32m+[m[32m3[m[41m[m
[32m+[m[32m–ë–æ–±[m[41m[m
[32m+[m[32m2[m[41m[m
[32m+[m[32m–í–∏—à–Ω—è[m[41m[m
[32m+[m[32m1[m[41m[m
[32m+[m[32m0[m[41m[m
[32m+[m[32m–Ø–±–ª–æ–∫–æ[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        ArrayList<String> list = new ArrayList<String>();[m[41m[m
[32m+[m[32m        while (true)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String s = reader.readLine();[m[41m[m
[32m+[m[32m            if (s.isEmpty()) break;[m[41m[m
[32m+[m[32m            list.add(s);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        String[] array = list.toArray(new String[list.size()]);[m[41m[m
[32m+[m[32m        sort(array);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (String x : array)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(x);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void sort(String[] array)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m            //–ù–∞–ø–∏—à–∏—Ç–µ —Ç—É—Ç –≤–∞—à –∫–æ–¥[m[41m[m
[32m+[m[32m            String temp = null;[m[41m[m
[32m+[m[32m            for (int i = 0; i < array.length; i++){[m[41m[m
[32m+[m[32m                for (int j = 0; j < i; j++){[m[41m[m
[32m+[m[32m                    if(isNumber(array[i]) && isNumber(array[j])){[m[41m[m
[32m+[m[32m                        Integer a = Integer.parseInt(array[i]);[m[41m[m
[32m+[m[32m                        Integer b = Integer.parseInt(array[j]);[m[41m[m
[32m+[m[32m                        Integer c = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        if(b < a){[m[41m[m
[32m+[m[32m                            c = a;[m[41m[m
[32m+[m[32m                            a = b;[m[41m[m
[32m+[m[32m                            b = c;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                            array[i] = Integer.toString(a);[m[41m[m
[32m+[m[32m                            array[j] = Integer.toString(b);[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    else if (!isNumber(array[i]) & !isNumber(array[j])){[m[41m[m
[32m+[m[32m                        if (!isGreaterThen(array[i].toLowerCase(), array[j].toLowerCase())){[m[41m[m
[32m+[m[32m                            temp = array[i];[m[41m[m
[32m+[m[32m                            array[i] = array[j];[m[41m[m
[32m+[m[32m                            array[j] = temp;[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    //–ú–µ—Ç–æ–¥ –¥–ª—è —Å—Ä–∞–≤–Ω–µ–Ω–∏—è —Å—Ç—Ä–æ–∫: '–∞' –±–æ–ª—å—à–µ —á–µ–º 'b'[m[41m[m
[32m+[m[32m    public static boolean isGreaterThen(String a, String b)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        return a.compareTo(b) > 0;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    //—Å—Ç—Ä–æ–∫–∞ - —ç—Ç–æ –Ω–∞ —Å–∞–º–æ–º –¥–µ–ª–µ —á–∏—Å–ª–æ?[m[41m[m
[32m+[m[32m    public static boolean isNumber(String s)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        if (s.length() == 0) return false;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        char[] chars = s.toCharArray();[m[41m[m
[32m+[m[32m        for (int i = 0; i < chars.length; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            char c = chars[i];[m[41m[m
[32m+[m[32m            if ((i != 0 && c == '-') //–µ—Å—Ç—å '-' –≤–Ω—É—Ç—Ä–∏ —Å—Ç—Ä–æ–∫–∏[m[41m[m
[32m+[m[32m                    || (!Character.isDigit(c) && c != '-') ) // –Ω–µ —Ü–∏—Ñ—Ä–∞ –∏ –Ω–µ –Ω–∞—á–∏–Ω–∞–µ—Ç—Å—è —Å '-'[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return false;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return true;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
