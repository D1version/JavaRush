[32m+[m[32mpackage com.javarush.test.level10.lesson11.bonus01;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.io.BufferedReader;[m[41m[m
[32m+[m[32mimport java.io.IOException;[m[41m[m
[32m+[m[32mimport java.io.InputStreamReader;[m[41m[m
[32m+[m[32mimport java.util.HashMap;[m[41m[m
[32m+[m[32mimport java.util.Map;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ù—É–∂–Ω–æ –∏—Å–ø—Ä–∞–≤–∏—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É, —á—Ç–æ–±—ã –∫–æ–º–ø–∏–ª–∏—Ä–æ–≤–∞–ª–∞—Å—å –∏ —Ä–∞–±–æ—Ç–∞–ª–∞[m[41m[m
[32m+[m[32m–ó–∞–¥–∞—á–∞: –ü—Ä–æ–≥—Ä–∞–º–º–∞ –¥–µ–º–æ–Ω—Å—Ç—Ä–∏—Ä—É–µ—Ç —Ä–∞–±–æ—Ç—É HashMap: –≤–≤–æ–¥–∏—Ç —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã –Ω–∞–±–æ—Ä –ø–∞—Ä (–Ω–æ–º–µ—Ä –∏ —Å—Ç—Ä–æ–∫—É), –ø–æ–º–µ—â–∞–µ—Ç –∏—Ö –≤ HashMap –∏ –≤—ã–≤–æ–¥–∏—Ç –Ω–∞ —ç–∫—Ä–∞–Ω —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ HashMap.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        HashMap<Integer, String> map = new HashMap<Integer, String>();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (int i = 0; i < 10; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int index = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m            String name = reader.readLine();[m[41m[m
[32m+[m[32m            map.put(index, name);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (Map.Entry<Integer, String> pair : map.entrySet())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int id = pair.getKey();[m[41m[m
[32m+[m[32m            String name = pair.getValue();[m[41m[m
[32m+[m[32m            System.out.println("Id=" + id + " Name=" + name);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
