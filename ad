[32m+[m[41m[m
[32m+[m[32mimport java.util.HashMap;[m[41m[m
[32m+[m[32mimport java.util.Map;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –õ—é–¥–∏ —Å –æ–¥–∏–Ω–∞–∫–æ–≤—ã–º–∏ –∏–º–µ–Ω–∞–º–∏ –∏/–∏–ª–∏ —Ñ–∞–º–∏–ª–∏—è–º–∏[m[41m[m
[32m+[m[32m1. –°–æ–∑–¥–∞—Ç—å —Å–ª–æ–≤–∞—Ä—å Map (<String, String>) –∏ –¥–æ–±–∞–≤–∏—Ç—å —Ç—É–¥–∞ 10 —á–µ–ª–æ–≤–µ–∫ –≤ –≤–∏–¥–µ ¬´–§–∞–º–∏–ª–∏—è¬ª-¬´–ò–º—è¬ª.[m[41m[m
[32m+[m[32m2. –ü—É—Å—Ç—å —Å—Ä–µ–¥–∏ —ç—Ç–∏—Ö 10 —á–µ–ª–æ–≤–µ–∫ –µ—Å—Ç—å –ª—é–¥–∏ —Å –æ–¥–∏–Ω–∞–∫–æ–≤—ã–º–∏ –∏–º–µ–Ω–∞–º–∏.[m[41m[m
[32m+[m[32m3. –ü—É—Å—Ç—å —Å—Ä–µ–¥–∏ —ç—Ç–∏—Ö 10 —á–µ–ª–æ–≤–µ–∫ –µ—Å—Ç—å –ª—é–¥–∏ —Å –æ–¥–∏–Ω–∞–∫–æ–≤—ã–º–∏ —Ñ–∞–º–∏–ª–∏—è–º–∏.[m[41m[m
[32m+[m[32m4. –í—ã–≤–µ—Å—Ç–∏ —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ Map –Ω–∞ —ç–∫—Ä–∞–Ω.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Map<String, String> map = createPeopleList();[m[41m[m
[32m+[m[32m        printPeopleList(map);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static Map<String, String> createPeopleList()[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        HashMap<String, String> map = new HashMap<String, String>();[m[41m[m
[32m+[m[32m        map.put("A", "Oleg");[m[41m[m
[32m+[m[32m        map.put("B", "Sasha");[m[41m[m
[32m+[m[32m        map.put("B", "Andrey");[m[41m[m
[32m+[m[32m        map.put("D", "Kolya");[m[41m[m
[32m+[m[32m        map.put("E", "Vasya");[m[41m[m
[32m+[m[32m        map.put("F", "Petya");[m[41m[m
[32m+[m[32m        map.put("G", "Andrey");[m[41m[m
[32m+[m[32m        map.put("H", "Oleg");[m[41m[m
[32m+[m[32m        map.put("I", "Sasha");[m[41m[m
[32m+[m[32m        map.put("J", "Andrey");[m[41m[m
[32m+[m[32m        return map;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void printPeopleList(Map<String, String> map)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        for (Map.Entry<String, String> s : map.entrySet())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            System.out.println(s.getKey() + " " + s.getValue());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
