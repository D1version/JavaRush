[32m+[m[32mpackage com.javarush.test.level05.lesson12.bonus02;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.io.BufferedReader;[m[41m[m
[32m+[m[32mimport java.io.InputStreamReader;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ù—É–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –≤ –ø—Ä–æ–≥—Ä–∞–º–º—É –Ω–æ–≤—É—é —Ñ—É–Ω–∫—Ü–∏–æ–Ω–∞–ª—å–Ω–æ—Å—Ç—å[m[41m[m
[32m+[m[32m–ó–∞–¥–∞—á–∞: –ü—Ä–æ–≥—Ä–∞–º–º–∞ –≤–≤–æ–¥–∏—Ç –¥–≤–∞ —á–∏—Å–ª–∞ —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã –∏ –≤—ã–≤–æ–¥–∏—Ç –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ –∏–∑ –Ω–∏—Ö –Ω–∞ —ç–∫—Ä–∞–Ω.[m[41m[m
[32m+[m[32m–ù–æ–≤–∞—è –∑–∞–¥–∞—á–∞: –ü—Ä–æ–≥—Ä–∞–º–º–∞ –≤–≤–æ–¥–∏—Ç –ø—è—Ç—å —á–∏—Å–µ–ª —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã –∏ –≤—ã–≤–æ–¥–∏—Ç –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ –∏–∑ –Ω–∏—Ö –Ω–∞ —ç–∫—Ä–∞–Ω.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        int a = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int b = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int c = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int d = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int e = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int minimum = min(a, b, c, d, e);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        System.out.println("Minimum = " + minimum);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static int min(int a, int b, int c, int d, int e)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        int m = a;[m[41m[m
[32m+[m[32m        if(b<m)[m[41m[m
[32m+[m[32m            m=b;[m[41m[m
[32m+[m[32m        if(c<m)[m[41m[m
[32m+[m[32m            m=c;[m[41m[m
[32m+[m[32m        if(d<m)[m[41m[m
[32m+[m[32m            m=d;[m[41m[m
[32m+[m[32m        if(e<m)[m[41m[m
[32m+[m[32m            m=e;[m[41m[m
[32m+[m[32m        return m;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
