[32m+[m[32m        processExceptions(new Solution());[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void processExceptions(Solution obj)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        try[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            obj.method1();[m[41m[m
[32m+[m[32m            obj.method2();[m[41m[m
[32m+[m[32m            obj.method3();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        catch (NullPointerException e)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            printStack(e);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        catch (IndexOutOfBoundsException e)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            printStack(e);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        catch (NumberFormatException e)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            printStack(e);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static void printStack(Throwable throwable) {[m[41m[m
[32m+[m[32m        System.out.println(throwable);[m[41m[m
[32m+[m[32m        for (StackTraceElement element : throwable.getStackTrace()) {[m[41m[m
[32m+[m[32m            System.out.println(element);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
