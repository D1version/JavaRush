[32m+[m[41m[m
[32m+[m[32m/* –ó–∞–¥–∞—á–∞ –ø–æ –∞–ª–≥–æ—Ä–∏—Ç–º–∞–º[m[41m[m
[32m+[m[32m–ù–∞–ø–∏—Å–∞—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É, –∫–æ—Ç–æ—Ä–∞—è:[m[41m[m
[32m+[m[32m1. –≤–≤–æ–¥–∏—Ç —Å –∫–æ–Ω—Å–æ–ª–∏ —á–∏—Å–ª–æ N > 0[m[41m[m
[32m+[m[32m2. –ø–æ—Ç–æ–º –≤–≤–æ–¥–∏—Ç N —á–∏—Å–µ–ª —Å –∫–æ–Ω—Å–æ–ª–∏[m[41m[m
[32m+[m[32m3. –≤—ã–≤–æ–¥–∏—Ç –Ω–∞ —ç–∫—Ä–∞–Ω –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–µ –∏–∑ –≤–≤–µ–¥–µ–Ω–Ω—ã—Ö N —á–∏—Å–µ–ª.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int N = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        int[] a = new int[N];[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (int i = 0; i < a.length; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int aa = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m            a[i] = aa;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        int maximum = a[0] ;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        for (int i = 0; i < a.length ; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if(maximum <= a[i])[m[41m[m
[32m+[m[32m                maximum = a[i];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        System.out.println(maximum);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
