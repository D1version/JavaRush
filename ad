[32m+[m[32mimport java.io.InputStreamReader;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ú–∞–º–∞ –ú—ã–ª–∞ –†–∞–º—É. –¢–µ–ø–µ—Ä—å —Å –±–æ–ª—å—à–æ–π –±—É–∫–≤—ã[m[41m[m
[32m+[m[32m–ù–∞–ø–∏—Å–∞—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É, –∫–æ—Ç–æ—Ä–∞—è –≤–≤–æ–¥–∏—Ç —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã —Å—Ç—Ä–æ–∫—É —Ç–µ–∫—Å—Ç–∞.[m[41m[m
[32m+[m[32m–ü—Ä–æ–≥—Ä–∞–º–º–∞ –∑–∞–º–µ–Ω—è–µ—Ç –≤ —Ç–µ–∫—Å—Ç–µ –ø–µ—Ä–≤—ã–µ –±—É–∫–≤—ã –≤—Å–µ—Ö —Å–ª–æ–≤ –Ω–∞ –∑–∞–≥–ª–∞–≤–Ω—ã–µ.[m[41m[m
[32m+[m[32m–í—ã–≤–µ—Å—Ç–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç –Ω–∞ —ç–∫—Ä–∞–Ω.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m–ü—Ä–∏–º–µ—Ä –≤–≤–æ–¥–∞:[m[41m[m
[32m+[m[32m  –º–∞–º–∞     –º—ã–ª–∞ —Ä–∞–º—É.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m–ü—Ä–∏–º–µ—Ä –≤—ã–≤–æ–¥–∞:[m[41m[m
[32m+[m[32m  –ú–∞–º–∞     –ú—ã–ª–∞ –†–∞–º—É.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        String s = reader.readLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        char[] array = s.toCharArray();[m[41m[m
[32m+[m[32m        for(int x=0;x<array.length;x++){[m[41m[m
[32m+[m[32m            if(x==0&&Character.isAlphabetic(array[0])){array[0]=Character.toUpperCase(array[0]);}[m[41m[m
[32m+[m[32m            else if(array[x]==' '&&Character.isAlphabetic(array[x+1])){array[x+1]=Character.toUpperCase(array[x+1]);}[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        for(char ch:array){[m[41m[m
[32m+[m[32m            System.out.print(ch);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m
