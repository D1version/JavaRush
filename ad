[32m+[m[32mpackage com.javarush.test.level08.lesson11.home04;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.io.BufferedReader;[m[41m[m
[32m+[m[32mimport java.io.IOException;[m[41m[m
[32m+[m[32mimport java.io.InputStreamReader;[m[41m[m
[32m+[m[32mimport java.util.ArrayList;[m[41m[m
[32m+[m[32mimport java.util.LinkedList;[m[41m[m
[32m+[m[32mimport java.util.List;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/* –ú–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ –∏–∑ N —á–∏—Å–µ–ª[m[41m[m
[32m+[m[32m1. –í–≤–µ—Å—Ç–∏ —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã —á–∏—Å–ª–æ N.[m[41m[m
[32m+[m[32m2. –°—á–∏—Ç–∞—Ç—å N —Ü–µ–ª—ã—Ö —á–∏—Å–µ–ª –∏ –∑–∞–ø–æ–ª–Ω–∏—Ç—å –∏–º–∏ —Å–ø–∏—Å–æ–∫ - –º–µ—Ç–æ–¥ getIntegerList.[m[41m[m
[32m+[m[32m3. –ù–∞–π—Ç–∏ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ —á–∏—Å–ª–æ —Å—Ä–µ–¥–∏ —ç–ª–µ–º–µ–Ω—Ç–æ–≤ —Å–ø–∏—Å–∫–∞ - –º–µ—Ç–æ–¥ getMinimum.[m[41m[m
[32m+[m[32m*/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mpublic class Solution[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public static void main(String[] args) throws Exception {[m[41m[m
[32m+[m[32m        List<Integer> integerList = getIntegerList();[m[41m[m
[32m+[m[32m        System.out.println(getMinimum(integerList));[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static int getMinimum(List<Integer> array) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int min = array.get(0);[m[41m[m
[32m+[m[32m        for (Integer elem : array)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (elem < min)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                min = elem;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return min;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static List<Integer> getIntegerList() throws IOException[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));[m[41m[m
[32m+[m[32m        int N = Integer.parseInt(reader.readLine());[m[41m[m
[32m+[m[32m        ArrayList<Integer> list = new ArrayList<Integer>();[m[41m[m
[32m+[m[32m        for (int i = 0; i < N; i++)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            list.add(Integer.parseInt(reader.readLine()));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return list;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
