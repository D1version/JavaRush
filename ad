[32m+[m[32m    public static int getCountTheSameFirstName(HashMap<String, String> map, String name)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        //–ù–∞–ø–∏—à–∏—Ç–µ —Ç—É—Ç –≤–∞—à –∫–æ–¥[m[41m[m
[32m+[m[32m        int count = 0;[m[41m[m
[32m+[m[32m        for (String tmp : map.values())[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if(name.equals(tmp))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                count++;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        return count;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    public static int getCountTheSameLastName(HashMap<String, String> map, String familiya)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        //–ù–∞–ø–∏—à–∏—Ç–µ —Ç—É—Ç –≤–∞—à –∫–æ–¥[m[41m[m
[32m+[m[32m        return map.containsKey(familiya) ? 1 : 0;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
